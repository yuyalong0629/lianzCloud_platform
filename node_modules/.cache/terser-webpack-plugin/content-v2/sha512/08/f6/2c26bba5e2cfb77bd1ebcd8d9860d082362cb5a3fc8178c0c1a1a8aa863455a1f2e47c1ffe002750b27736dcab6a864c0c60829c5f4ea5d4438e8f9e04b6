\\\" @click=\\\"e=>updataDes(e,index)\\\" />\\n                  </a-popover>\\n                </div>\\n              </div>\\n              <div\\n                class=\\\"list-item\\\"\\n                :class=\\\"selectIndex===index?'active':''\\\"\\n                @click=\\\"addCompany(item.id,index)\\\"\\n                v-else\\n              >\\n                <div class=\\\"item-left\\\">\\n                  <div class=\\\"item-left-top\\\">\\n                    <a href=\\\"javascript:;\\\">{{item.factoryName}}</a>\\n                    <span>{{item.factoryOdNumber}}</span>\\n                  </div>\\n                  <ul class=\\\"item-left-desc\\\">\\n                    <li>{{item.name}}</li>\\n                    <li>{{item.spec}}</li>\\n                    <li>{{item.cz}}</li>\\n                    <li>{{item.odNumber}} {{item.unit}}</li>\\n                  </ul>\\n                </div>\\n                <div class=\\\"item-right\\\">\\n                  <a-button type=\\\"primary\\\" shape=\\\"circle\\\" icon=\\\"plus\\\"></a-button>\\n                </div>\\n              </div>\\n            </a-list-item>\\n            <div v-if=\\\"loadingCompony && !busy\\\" class=\\\"demo-loading-container\\\">\\n              <a-spin />\\n            </div>\\n          </a-list>\\n        </div>\\n      </a-col>\\n      <a-col :span=\\\"12\\\" class=\\\"dispatch-right procurement\\\">\\n        <div class=\\\"title\\\"></div>\\n        <div class=\\\"order-list demo-infinite-container\\\">\\n          <a-list :dataSource=\\\"companyList\\\">\\n            <a-list-item slot=\\\"renderItem\\\" slot-scope=\\\"item\\\">\\n              <div class=\\\"list-item\\\" v-if=\\\"companyType === 'CHANNEL'\\\">\\n                <div class=\\\"item-left\\\">\\n                  <div class=\\\"item-left-top\\\">\\n                    <a href=\\\"javascript:;\\\">{{item.odSkuName}}</a>\\n                    <span\\n                      v-if=\\\"item.bomGroupId && i