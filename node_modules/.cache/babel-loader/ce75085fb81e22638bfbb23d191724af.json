apper-col=\"wrapperCol\" label=\"客户名称\">\n              <a-input v-model=\"custName\" @change=\"(val)=>{inputChange(val,'custName')}\"></a-input>\n            </a-form-item>\n          </a-col>\n        </a-row>\n        <div\n          class=\"order-list order-list-item demo-infinite-container\"\n          v-infinite-scroll=\"handleInfiniteOnLoad\"\n          :infinite-scroll-disabled=\"busy\"\n          :infinite-scroll-distance=\"10\"\n        >\n          <a-list :dataSource=\"dataCompany\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n              <div\n                class=\"list-item\"\n                :class=\"selectIndex===index?'active':''\"\n                @click=\"addCompany(item.id,index)\"\n              >\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a href=\"javascript:;\">{{item.ocustShortName}}{{item.ocustCode}}</a>\n                    <span>{{item.oorderNumber}}</span>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.oaddresseeName}}</li>\n                    <li>{{item.ophone}}</li>\n                  </ul>\n                </div>\n                <div class=\"item-right\">\n                  <a-popover trigger=\"click\" v-model=\"item.clickedVisible\">\n                    <template slot=\"content\">\n                      <a-form-item\n                        label=\"紧急程度:\"\n                        v-model=\"item.item\"\n                        :label-col=\"{ span: 12}\"\n                        :wrapper-col=\"{span:12}\"\n                      >\n                        <a-switch/>\n                      </a-form-item>\n                      <a-form-item class=\"mb\">\n                        <a-button type=\"danger\" @click=\"removeItem(item.id)\">移除</a-button>\n                      </a-form-item>\n                    </template>\n                    <a-icon type=\"ellipsis\" class=\"item-right-edit\" @click=\"updataDes(index)\"/>\n                  </a-popover>\n                </div>\n              </div>\n            </a-list-item>\n            <div v-if=\"loadingCompony && !busy\" class=\"demo-loading-container\">\n              <a-spin/>\n            </div>\n          </a-list>\n        </div>\n      </a-col>\n      <a-col :span=\"12\" class=\"dispatch-right procurement\">\n        <div class=\"title\"></div>\n        <div class=\"order-list demo-infinite-container\">\n          <a-list :dataSource=\"companyList\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item\">\n              <div class=\"list-item\">\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a href=\"javascript:;\">{{item.odSkuName}}</a>\n                    <span\n                      v-if=\"item.bomGroupId && item.craftGroupId && item.skuPriceInventory\"\n                    >库存：{{item.skuPriceInventory.storeCount}}</span>\n                    <span v-else>库存：{{item.sku.storeCount}}</span>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.odSpec}}</li>\n                    <li>{{item.sku.cz}}</li>\n                    <li>{{item.odUndeliveredNumber}}</li>\n                    <li>{{item.odRequireDueDate | formatDate}}</li>\n                  </ul>\n                </div>\n                <div class=\"item-right\">\n                  <a-input-number\n                    :min=\"0\"\n                    :max=\"Number(item.odUndeliveredNumber)\"\n                    v-model=\"item.odNumber\"\n                  />\n                </div>\n              </div>\n            </a-list-item>\n          </a-list>\n          <div style=\"height:40px\"></div>\n          <div class=\"dispatch-right-bottom\">\n            <a-button @click=\"cancelSubmit\">取消</a-button>\n            <a-button type=\"primary\" @click=\"submit\" :loading=\"submitLoading\">发货</a-button>\n          </div>\n        </div>\n      </a-col>\n    </a-row>\n  </div>\n</template>\n\n<script>\nimport infiniteScroll from 'vue-infinite-scroll'\nimport { get, postJson } from '@/api/axios.js'\nimport api from '@/api/index.js'\nimport { mixinBasic } from '@/components/tools/mixin/mixin'\nimport { mixinCurrentPage } from '@/views/sale/mixin/minxinSale'\nexport default {\n  directives: { infiniteScroll },\n  mixins: [mixinBasic, mixinCurrentPage],\n  data() {\n    return {\n      busy: false,\n      companyVisible: false,\n      loadingCompony: false,\n      dataCompanySize: 0,\n      dataCompany: [],\n      currentPage: 1,\n      pageSize: 15,\n      custName: '',\n      companyList: [],\n      selectIndex: '',\n      submitLoading: false\n    }\n  },\n  created() {\n  },\n  methods: {\n    inputChange(val, type) {\n      this.dataCompany = []\n      this.custName = val\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.getFactoryOrder()\n      }, 1000)\n    },\n    handleInfiniteOnLoad() {\n      const data = this.dataCompany\n      this.loadingCompony = true\n      this.busy = true\n      if (data.length >= this.dataCompanySize && this.dataCompanySize !== 0) {\n        this.busy = true\n        this.loadingCompony = false\n      } else {\n        this.getFactoryOrder()\n        this.busy = false\n        this.loadingCompony = false\n      }\n    },\n    removeItem(id) {\n      this.$confirm({\n        title: '移除订单信息',\n        content: '您确定要移除这条订单消息吗?',\n        zIndex: 99999,\n        onOk() {\n        },\n        onCancel() { }\n      })\n    },\n    addCompany(id, index) {\n      this.selectIndex = index\n      const dataCompany = this.dataCompany\n      this.companyList = dataCompany[index].skuList\n    },\n    minusCompany(id, index) {\n    },\n    getFactoryOrder(currentPage = 1, pageSize = 15) {\n      const data = {}\n      data.currentPage = currentPage\n      data.pageSize = pageSize\n      data.custName = this.custName\n      get(api.outStorageApplyTobedistributed, data).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data.records\n          data.forEach(item => {\n            item.clickedVisible = false\n            item['requireLevel'] = false\n          })\n          this.dataCompanySize = Number(res.data.data.total)\n          this.dataCompany = this.dataCompany.concat(data)\n          this.currentPage += 1\n        }\n      })\n    },\n    updataDes(index) {\n      this.dataCompany[index].clickedVisible = true\n    },\n    submit() {\n      const companyList = this.companyList\n      if (companyList.length === 0) { return false }\n      companyList.forEach(item => {\n        if (item.bomGroupId && item.craftGroupId && item.skuPriceInventory) {\n          if (item.odNumber > item.skuPriceInventory.storeCount) {\n            this.prompt()\n          }\n        } else {\n          if (item.odNumber > item.sku.storeCount) {\n            this.prompt()\n          }\n        }\n      })\n    },\n    // 发货请求\n    deliveryRequest(data) {\n      this.submitLoading = true\n      postJson(api.outStorageApplyOpt, data).then(res => {\n        if (res.data.code === 0) {\n          this.dataCompany = []\n          this.companyList = []\n          this.selectIndex = ''\n          this.getFactoryOrder()\n          this.$message.success('发货成功')\n        } else {\n          this.$message.error(res.data.msg)\n        }\n        this.submitLoading = false\n      })\n    },\n    // 发货数据处理\n    dataSplic() {\n      const data = []\n      const companyList = this.companyList\n      const dataCompany = this.dataCompany[this.selectIndex]\n      companyList.forEach(item => {\n        data.push(\n          {\n            oid: dataCompany.id,\n            odId: item.id,\n            skuId: item.skuId,\n            requireLevel: dataCompany.requireLevel ? 'LEVEL1' : 'LEVEL0',\n            reciverId: item.odCustId,\n            reciverName: dataCompany.ocustShortName,\n            oaddresseeName: dataCompany.oaddresseeName,\n            ophone: dataCompany.ophone,\n            oaddress: dataCompany.olocation + dataCompany.ostreet,\n            outCount: item.odNumber,\n            packageInfo: ''\n          }\n        )\n      })\n      return data\n    },\n    // 库存不足发货提示\n    prompt() {\n      const _this = this\n      this.$confirm({\n        title: '通知',\n        content: '库存量不足，是否还要发货？',\n        onOk() {\n          const data = _this.dataSplic()\n          _this.deliveryRequest(data)\n        },\n        onCancel() {\n        }\n      })\n    },\n    cancelSubmit() {\n      this.companyList = []\n      this.selectIndex = []\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/dispach.less\";\n</style>\n"],"sourceRoot":"src/views/sale/delivery"}]}