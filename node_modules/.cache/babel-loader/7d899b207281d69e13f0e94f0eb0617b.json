      this.openKeys = latestOpenKey ? [latestOpenKey] : []\n      }\n    },\n    onSelectKey(e) {\n      console.log(e)\n      defaultPermission(e.key)\n      this.$store.commit('setFuncIdArray', e.key)\n    }\n  },\n  watch: {\n    '$route.path': function(val) {\n      console.log(val)\n    },\n    isCollapsed(val) {\n      this.openKeys = !val ? this.openKeys : []\n    }\n  },\n  components: {\n    'sub-menu': SubMenu,\n    'icon-font': IconFont\n  }\n}\n</script>\n"],"sourceRoot":"src/components/menu"}]}