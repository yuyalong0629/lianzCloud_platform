wer>\n    </div>\n    <!-- 产品分类 -->\n    <div class=\"goods-tab\">\n      <!-- Tree -->\n      <div class=\"goods-tree\" style=\"height: 75vh; overflow-y: scroll;\">\n        <a-directory-tree @expand=\"onExpand\" @select=\"onSelect\" :treeData=\"treeData\"/>\n      </div>\n      <!-- Table -->\n      <div class=\"goods-list\">\n        <a-table\n          class=\"goods-list\"\n          :pagination=\"pagination\"\n          :columns=\"columns\"\n          :dataSource=\"tabData\"\n          :scroll=\"scrollLen\"\n        >\n          <a\n            slot=\"code\"\n            slot-scope=\"text, record\"\n            href=\"javascript:;\"\n            @click=\"onHanldeGoodsInfo(record)\"\n          >{{ text }}</a>\n        </a-table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  getGoodsTree,\n  searchGoods,\n  getTabHeader,\n  getGoodsViews\n} from './goods.js'\nimport GoodsInfo from './GoodsInfo'\n\nexport default {\n  name: 'goods',\n  data() {\n    return {\n      scrollLen: { x: 800, y: '68vh' },\n      pagination: {\n        total: 0,\n        defaultPageSize: 15,\n        defaultCurrent: 1,\n        showQuickJumper: true,\n        hideOnSinglePage: true\n      },\n      // table\n      tabData: [],\n      columns: [\n        {\n          title: '产品编码',\n          dataIndex: 'code',\n          key: 'code',\n          width: 120,\n          fixed: 'left'\n        },\n        {\n          title: '上下架',\n          dataIndex: 'saleStatus',\n          key: 'saleStatus',\n          width: 120,\n          fixed: 'left'\n        },\n        {\n          title: '产品名称',\n          dataIndex: 'name',\n          key: 'name',\n          width: 120,\n          scopedSlots: { customRender: 'name' },\n          fixed: 'left'\n        },\n        {\n          title: '规格',\n          dataIndex: 'spec',\n          key: 'spec',\n          width: 120,\n          fixed: 'left'\n        },\n        {\n          title: '单位',\n          dataIndex: 'nuit',\n          key: 'nuit',\n          width: 120,\n          fixed: 'left'\n        },\n        {\n          title: '客户编码',\n          dataIndex: 'customCode',\n          key: 'customCode',\n          width: 120\n        },\n        {\n          title: '价格',\n          dataIndex: 'price',\n          key: 'price',\n          width: 120\n        },\n        {\n          title: '时间',\n          key: 'creatTime',\n          width: 120,\n          dataIndex: 'creatTime',\n          fixed: 'right'\n        }\n      ],\n      loading: false,\n      visible: false,\n      treeData: [],\n      addCloumn: [],\n      isCatId: '',\n      isCustId: '',\n      addGoodsId: '',\n      // Goods详情\n      setGoodsViews: {},\n      isEdit: false,\n      isPermission: true\n    }\n  },\n  created() {\n    getGoodsTree.call(this)\n  },\n  mounted() {\n    // 初始化客户信息\n    // showCustomersPage.call(this)\n  },\n  methods: {\n    // tree\n    onSelect(selectedKeys, info) {\n      console.log(info.node.dataRef.custundefined)\n      this.isCustId =\n        info.node.dataRef.custundefined === null\n          ? '0'\n          : info.node.dataRef.custundefined\n      // 判断是否存在字母\n      const isLetter = /[a-z]/i\n      if (!isLetter.test(selectedKeys[0])) {\n        console.log(selectedKeys)\n        const params = {\n          catIds: selectedKeys,\n          pageNo: '1',\n          pageSize: '10'\n        }\n        this.isCatId = selectedKeys[0]\n        getTabHeader.call(this, { catId: selectedKeys[0] }).then(() => {\n          searchGoods.call(this, params)\n        })\n      }\n    },\n    onExpand() {\n      console.log('Trigger Expand')\n    },\n    onDeleteProgoods() {},\n    showDrawer() {\n      if (this.isCatId !== '') {\n        this.visible = true\n        this.isEdit = false\n        return false\n      }\n      this.$message.error('请先选择分类')\n    },\n    onClose() {\n      this.visible = false\n    },\n    // $emit改变isvisible\n    isVisible(isVisible) {\n      this.visible = isVisible\n    },\n    // 商品详情\n    onHanldeGoodsInfo(record) {\n      console.log(record)\n      if (this.isPermission) {\n        getGoodsViews.call(this, record.key)\n        this.visible = true\n        this.isEdit = true\n      }\n    },\n    // 权限\n    controlPermission(val) {\n      val.forEach(item => {\n        // console.log(item.btnOptType)\n        switch (item.btnOptType) {\n          case 'LIST':\n            // 客户搜索\n            console.log('LIST')\n            this.isPermission = true\n            break\n          case 'IMPORT':\n            // 导入\n            console.log('IMPORT')\n            this.isPermission = true\n            break\n          case 'EXPORT':\n            // 导出\n            console.log('EXPORT')\n            this.isPermission = true\n            break\n          case 'DELETES':\n            // 删除\n            console.log('DELETES')\n            this.isPermission = true\n            break\n          case 'ADD':\n            // 新建\n            console.log('ADD')\n            this.isPermission = true\n            break\n          case 'DETAIL':\n            // 详情\n            console.log('DETAIL')\n            this.isPermission = true\n            break\n          default:\n            console.log('other')\n            this.isPermission = false\n        }\n      })\n    }\n  },\n  computed: {\n    getNavPermission() {\n      return this.$store.getters.getNavPermission\n    }\n  },\n  watch: {\n    getNavPermission(val) {\n      // console.log(val)\n      this.controlPermission(val)\n    }\n  },\n  components: {\n    GoodsInfo\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.goods {\n  .goods-add {\n    margin-bottom: 15px;\n  }\n  .goods-remove {\n    margin: 0 10px;\n  }\n  .goods-select-reset {\n    margin: 0 10px;\n  }\n  .goods-select {\n    margin: 16px 0;\n  }\n  .goods-tab {\n    display: flex;\n    .goods-tree {\n      border: 1px solid #eeeeee;\n      flex: 0 0 200px;\n      margin-right: 15px;\n    }\n    .goods-list {\n      flex