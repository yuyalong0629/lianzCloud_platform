label-col=\"formItemLayout.labelCol\"\n            :wrapper-col=\"formItemLayout.wrapperCol\"\n          >\n            <a-select\n              placeholder=\"请选择商品编码\"\n              @change=\"(val)=>{inputChange(val)}\"\n              @popupScroll=\"e=>currentPageChange(e)\"\n              :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n            >\n              <a-select-option v-for=\"(item,index) in odSkuCodeList\" :key=\"index\">{{item}}</a-select-option>\n            </a-select>\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"24\">\n          <a-form-item\n            label=\"订单数量\"\n            :label-col=\"formItemLayout.labelCol\"\n            :wrapper-col=\"formItemLayout.wrapperCol\"\n          >\n            <a-input\n\n              v-model=\"sku.odNumber\"\n              placeholder=\"请输入订单数量\"\n              @change=\"(e)=>{inputnumChange(e.target.value)}\"\n            />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"24\">\n          <a-form-item\n            label=\"采购单号：\"\n            :label-col=\"formItemLayout.labelCol\"\n            :wrapper-col=\"formItemLayout.wrapperCol\"\n          >\n            <a-input placeholder=\"请输入采购单号\" v-model=\"sku.odPurchaseOrderno\" />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"24\">\n          <a-form-item\n            label=\"订单总计\"\n            :label-col=\"formItemLayout.labelCol\"\n            :wrapper-col=\"formItemLayout.wrapperCol\"\n          >\n            <a-input v-model=\"sku.odAmount\" placeholder=\"请输入订单总计\" />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"24\">\n          <a-form-item\n            label=\"要求交期\"\n            :label-col=\"formItemLayout.labelCol\"\n            :wrapper-col=\"formItemLayout.wrapperCol\"\n          >\n            <a-date-picker @change=\"odRequireDueDate\" placeholder=\"请选择要求交期\" />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"24\">\n          <a-form-item\n            label=\"承诺交期\"\n            :label-col=\"formItemLayout.labelCol\"\n            :wrapper-col=\"formItemLayout.wrapperCol\"\n          >\n            <a-date-picker @change=\"odPromiseDueDate\" placeholder=\"请选择承诺交期\" />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"22\" :style=\"{ textAlign: 'right' }\">\n          <a-form-item>\n            <a-button :style=\"{ marginRight: '8px' }\" @click=\"handleReset\">取消</a-button>\n            <a-button type=\"primary\" @click=\"handleEditableInfo\">确认添加</a-button>\n          </a-form-item>\n        </a-col>\n      </a-row>\n    </a-form>\n  </div>\n</template>\n<script>\nconst formItemLayout = {\n  labelCol: { span: 6, offset: 0 },\n  wrapperCol: { span: 16 }\n}\nimport {\n  getListCustomerGoods\n} from './menu.js'\nexport default {\n  props: ['custId'],\n  data() {\n    return {\n      customerGoods: 1, // 列表商家对客户的定制商品基本信息\n      formItemLayout,\n      edit: true,\n      sku: {\n        skuId: '',\n        code: '',\n        name: '',\n        spec: '',\n        taxRate: 0,\n        price: 0,\n        odNumber: 0,\n        odAmount: 0,\n        customCode: '', // 客户产品编码\n        odPurchaseOrderno: '',\n        odRequireDueDate: '',\n        odPromiseDueDate: ''\n      },\n      listCustomerGoods: [], //商品信息\n      odSkuCodeList: [], // 产品编码\n      odSpecList: [] // 产品规格\n    }\n  },\n  created() {\n    getListCustomerGoods.call(this, this.custId, this.customerGoods)\n  },\n  methods: {\n    inputChange(val) {\n      this.dataHandle(val)\n      this.edit = false\n    },\n    dataHandle(index) {\n      var listCustomerGoods = this.listCustomerGoods[index]\n      this.sku.name = listCustomerGoods.name\n      this.sku.spec = listCustomerGoods.spec\n      this.sku.taxRate = listCustomerGoods.taxRate\n      this.sku.price = listCustomerGoods.price\n      this.sku.code = listCustomerGoods.code\n      this.sku.customCode = listCustomerGoods.customCode\n      this.sku.skuId = listCustomerGoods.id\n    },\n    inputnumChange(val) {\n      var price = this.sku.price\n      let total = 0\n      total = (price * val).toFixed(3)\n      this.sku.odAmount = total\n    },\n    handleEditableInfo() {\n      if (this.edit) {\n        this.$message.error('请选择产品编号')\n        return\n      }\n      var data = {}\n      data.isAddContactInfo = false\n      data.sku = this.sku\n      this.$emit('showAddContactInfo', data)\n      this.$message.success('添加成功')\n    },\n    handleReset() {\n      var data = {}\n      data.isAddContactInfo = false\n      this.$emit('showAddContactInfo', data)\n    },\n    onSelectCustSex(value) {\n      console.log(value)\n    },\n    odPromiseDueDate(value, dateString) {\n      this.sku.odPromiseDueDate = dateString\n    },\n    odRequireDueDate(value, dateString) {\n      this.sku.odRequireDueDate = dateString\n    },\n    currentPageChange(e) {\n      const boxHeigth = e.target.scrollHeight - 1\n      const box = {\n        scrollTop: e.target.scrollTop,\n      }\n      if (parseFloat(getComputedStyle(document.querySelector('.ant-select-dropdown-content')).height) + box.scrollTop > boxHeigth) {\n        getListCustomerGoods.call(this, this.custId, this.customerGoods)\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/sale/orderList"}]}