nfo/CategoryInfo';\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin';\nimport { columns } from './basic_lib/column';\nimport { getCategoryDetail, getGoodsList } from './basic_lib/index';\nimport { setTimeout } from 'timers';\nexport default {\n  name: 'Category',\n  mixins: [mixinBasic, mixinDrawer],\n  data: function data() {\n    return {\n      data: [],\n      columns: columns,\n      categoryInfo: {}\n    };\n  },\n  created: function created() {\n    // 初始化Tab数据\n    getGoodsList.call(this);\n  },\n  methods: {\n    // 查看详情\n    onHandleInfo: function onHandleInfo(record) {\n      var _this = this;\n\n      getCategoryDetail.call(this, record.id).then(function () {\n        _this.visible = true;\n      });\n      this.$store.commit('setPromptTitle', {\n        title: '产品分类详情',\n        isEdit: true\n      });\n    },\n    // $emit改变isvisible\n    isDrawer: function isDrawer(val) {\n      var _this2 = this;\n\n      this.visible = val; // 关闭抽屉主动初始化\n\n      this.timer = setTimeout(function () {\n        getGoodsList.call(_this2);\n      }, 100);\n    },\n    // 新建\n    showDrawer: function showDrawer() {\n      this.visible = true; // this.categoryInfo = {}\n\n      this.$store.commit('setPromptTitle', {\n        title: '新建产品分类',\n        isEdit: false\n      });\n    }\n  },\n  watch: {\n    '$store.state.menuPermission': {\n      handler: function handler(val) {\n        console.log(val);\n      },\n      deep: true,\n      immediate: true\n    }\n  },\n  components: {\n    'category-info': CateGoryInfo\n  }\n};",{"version":3,"sources":["Category.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwCA,OAAA,YAAA,MAAA,2BAAA;AACA,SAAA,UAAA,EAAA,WAAA,QAAA,gCAAA;AACA,SAAA,OAAA,QAAA,oBAAA;AACA,SAAA,iBAAA,EAAA,YAAA,QAAA,mBAAA;AACA,SAAA,UAAA,QAAA,QAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,UADA;AAEA,EAAA,MAAA,EAAA,CAAA,UAAA,EAAA,WAAA,CAFA;AAGA,EAAA,IAHA,kBAGA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,OAAA,EAAA,OAFA;AAGA,MAAA,YAAA,E