 :wrapStyle=\"{height: 'calc(100% - 108px)',overflow: 'auto',paddingBottom: '108px'}\"\n    >\n      <category-info @isDrawer=\"isDrawer\" :categoryInfo=\"categoryInfo\"/>\n    </a-drawer>\n  </div>\n</template>\n<script>\nimport CateGoryInfo from './child_info/CategoryInfo'\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin'\nimport { columns } from './basic_lib/column'\nimport { getCategoryDetail, getGoodsList } from './basic_lib/index'\nimport { setTimeout } from 'timers';\n\nexport default {\n  name: 'Category',\n  mixins: [mixinBasic, mixinDrawer],\n  data() {\n    return {\n      data: [],\n      columns,\n      categoryInfo: {}\n    }\n  },\n  created() {\n    // 初始化Tab数据\n    getGoodsList.call(this)\n  },\n  methods: {\n    // 查看详情\n    onHandleInfo(record) {\n      getCategoryDetail.call(this, record.id).then(() => {\n        this.visible = true\n      })\n      this.$store.commit('setPromptTitle', {\n        title: '产品分类详情',\n        isEdit: true\n      })\n    },\n    // $emit改变isvisible\n    isDrawer(val) {\n      this.visible = val\n      // 关闭抽屉主动初始化\n      this.timer = setTimeout(() => {\n        getGoodsList.call(this)\n      }, 100)\n    },\n    // 新建\n    showDrawer() {\n      this.visible = true\n      // this.categoryInfo = {}\n      this.$store.commit('setPromptTitle', {\n        title: '新建产品分类',\n        isEdit: false\n      })\n    }\n  },\n  watch: {\n    '$store.state.menuPermission': {\n      handler(val) {\n        console.log(val)\n      },\n      deep: true,\n      immediate: true\n    }\n  },\n  components: {\n    'category-info': CateGoryInfo\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.category {\n  .category-setup {\n    margin-bottom: 15px;\n  }\n  .category-remove {\n    margin: 0 10px;\n  }\n  .category-select-reset {\n    margin: 0 10px;\n  }\n  .category-select {\n    margin: 16px 0;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/product/bom/category"}]}