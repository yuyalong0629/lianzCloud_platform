EAAA;AACA,QAAA,IAAA,CAAA,IAAA,CACA;AACA,UAAA,EAAA,EAAA,IAAA,CAAA,EADA;AAEA,UAAA,MAAA,EAAA,IAAA,CAAA,QAFA;AAGA,UAAA,WAAA,EAAA,IAAA,CAAA,WAHA;AAIA,UAAA,gBAAA,EAAA,IAAA,CAAA;AAJA,SADA;AAQA,OATA;AAUA,WAAA,aAAA,GAAA,IAAA;AACA,MAAA,QAAA,CAAA,GAAA,CAAA,eAAA,EAAA,2BAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,0BAAA,CAAA,WAAA,GAAA,CAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,EAAA;;AACA,UAAA,MAAA,CAAA,eAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OAXA;AAYA,KA9LA;AA+LA;AACA,IAAA,UAhMA,sBAgMA,IAhMA,EAgMA;AACA,WAAA,SAAA,GAAA,IAAA,CAAA,EAAA;AACA,WAAA,WAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,KApMA;AAqMA;AACA,IAAA,QAtMA,oBAsMA,GAtMA,EAsMA;AACA,WAAA,OAAA,GAAA,GAAA;AACA,KAxMA;AAyMA;AACA,IAAA,QA1MA,oBA0MA,EA1MA,EA0MA;AACA,WAAA,WAAA,CAAA,EAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA;AAIA,KAhNA;AAiNA,IAAA,WAjNA,uBAiNA,MAjNA,EAiNA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,SAAA,oBAAA,MAAA,CAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA;AACA,OALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAPA;AAQA,KA1NA;AA2NA;AACA,IAAA,iBA5NA,6BA4NA,CA5NA,EA4NA,IA5NA,EA4NA;AACA,UAAA,MAAA,GAAA,KAAA,oBAAA,CAAA,CAAA,CAAA;;AACA,UAAA,MAAA,EAAA;AACA,YAAA,IAAA,KAAA,IAAA,EAAA;AACA,eAAA,sBAAA,CAAA,KAAA,aAAA;AACA,SAFA,MAEA,IAAA,IAAA,KAAA,MAAA,EAAA;AACA,eAAA,yBAAA,CAAA,KAAA,eAAA;AACA;AACA;AACA;AArOA;AAxCA,CAAA","sourcesContent":["<template>\n  <div class=\"dispatch\">\n    <a-row>\n      <a-col :span=\"12\" class=\"dispatch-left\">\n        <a-row class=\"search\">\n          <a-col :span=\"8\">\n            <a-form-item label=\"规格\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-select\n                style=\"width:160px\"\n                showSearch\n                :allowClear=\"true\"\n                placeholder=\"请选择规格\"\n                @change=\"(val)=>{inputChange(val,'spec')}\"\n                @popupScroll=\"e=>currentPageChange(e,'spec')\"\n                :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n              >\n                <a-select-option v-for=\"item in odSpecList\" :key=\"item\">{{item}}</a-select-option>\n              </a-select>\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"8\">\n            <a-form-item label=\"交期\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-range-picker\n                style=\"width:160px\"\n                format=\"YYYY-MM-DD HH:mm:ss\"\n                @change=\"(dates, dateStrings)=>{inputChange(dateStrings,'date')}\"\n              />\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"8\">\n            <a-form-item label=\"材质\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-select\n                style=\"width:160px\"\n                showSearch\n                :allowClear=\"true\"\n                placeholder=\"请选择材质\"\n                @change=\"(val)=>{inputChange(val,'cz')}\"\n                @popupScroll=\"e=>currentPageChange(e,'cz')\"\n                :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n              >\n                <a-select-option v-for=\"item in textures\" :key=\"item\">{{item}}</a-select-option>\n              </a-select>\n            </a-form-item>\n          </a-col>\n        </a-row>\n        <div\n          class=\"order-list order-list-item demo-infinite-container\"\n          v-infinite-scroll=\"handleInfiniteOnLoad\"\n          :infinite-scroll-disabled=\"busy\"\n          :infinite-scroll-distance=\"10\"\n        >\n          <a-list :dataSource=\"dataCompany\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n              <div class=\"list-item\">\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a\n                      href=\"javascript:;\"\n                      @click=\"infoView(item.channelSkuId)\"\n                    >{{item.name}} {{item.bom}}</a>\n                    <span>{{item.craft}}</span>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.spec}}</li>\n                    <li>{{item.cz}}</li>\n                    <li>{{item.bmcl}}</li>\n                    <li>{{moment(item.odPromiseDueDate).format(dateFormat)}}</li>\n                    <li>{{item.odNumber}}{{item.unit}}</li>\n                  </ul>\n                </div>\n                <div class=\"item-right\">\n                  <a-popover trigger=\"click\" v-model=\"item.clickedVisible\">\n                    <template slot=\"content\">\n                      <a-form-item\n                        label=\"交期\"\n                        :label-col=\"{ span: 5,offset:0 }\"\n                        :wrapper-col=\"{span:12}\"\n                        class=\"mb\"\n                      >\n                        <a-date-picker\n                          showTime\n                          format=\"YYYY-MM-DD HH:mm:ss\"\n                          :defaultValue=\"moment(updateList.date||new Date(), 'YYYY-MM-DD HH:mm:ss')\"\n                          style=\"width:170px;\"\n                          @change=\"updateTime\"\n                        />\n                      </a-form-item>\n                      <a-form-item\n                        label=\"数量\"\n                        :label-col=\"{ span: 5,offset:0 }\"\n                        :wrapper-col=\"{span:12}\"\n                        class=\"mb\"\n                      >\n                        <a-input-number\n                          :min=\"1\"\n                          :max=\"item.odNumber\"\n                          v-model=\"updateList.num\"\n                          style=\"width:170px;\"\n                        />\n                      </a-form-item>\n                      <a-form-item class=\"mb\">\n                        <a-button type=\"danger\" @click=\"removeItem(item.id,index)\">移除</a-button>\n                      </a-form-item>\n                      <a-form-item class=\"mb\">\n                        <a-button type=\"primary\" @click=\"addCompany(item.id,index,'addLittle')\">确定</a-button>\n                      </a-form-item>\n                    </template>\n                    <a-icon\n                      type=\"ellipsis\"\n                      class=\"item-right-edit\"\n                      @click=\"updataDes(item.odNumber,item.odPromiseDueDate,index)\"\n                    />\n                  </a-popover>\n                  <a-button\n                    type=\"primary\"\n                    shape=\"circle\"\n                    icon=\"plus\"\n                    @click=\"addCompany(item.id,index,'addAll')\"\n                  ></a-button>\n                </div>\n              </div>\n            </a-list-item>\n            <div v-if=\"loadingCompony && !busy\" class=\"demo-loading-container\">\n              <a-spin/>\n            </div>\n          </a-list>\n        </div>\n      </a-col>\n      <a-col :span=\"12\" class=\"dispatch-right\">\n        <div class=\"title\">\n          <span @click=\"showDrawer\">{{companyName}}</span>\n          <span @click=\"showDrawer\" v-if=\"companyVisible\">\n            <a-icon type=\"double-right\"/>\n          </span>\n          <span @click=\"showDrawer\">\n            <a-icon type=\"double-left\"/>\n          </span>\n        </div>\n        <div class=\"order-list demo-infinite-container\">\n          <a-list :dataSource=\"companyList\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n              <div class=\"list-item\">\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a href=\"javascript:;\">{{item.name}} {{item.bom}}</a>\n                    <span>{{item.craft}}</span>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.spec}}</li>\n                    <li>{{item.cz}}</li>\n                    <li>{{item.bmcl}}</li>\n                    <li>{{moment(item.odPromiseDueDate).format(dateFormat)}}</li>\n                    <li>{{item.odNumber}}{{item.unit}}</li>\n                  </ul>\n                </div>\n                <div class=\"item-right\">\n                  <a-button\n                    type=\"danger\"\n                    shape=\"circle\"\n                    icon=\"minus\"\n                    style=\"background:#f5222d;color:#fff;\"\n                    @click=\"minusCompany(item.id,index)\"\n                  ></a-button>\n                </div>\n              </div>\n            </a-list-item>\n          </a-list>\n          <div style=\"height:40px\"></div>\n          <div class=\"dispatch-right-bottom\">\n            <a-button @click=\"handleClose\">取消</a-button>\n            <a-button type=\"primary\" @click=\"submit\" :loading=\"submitLoading\">确定</a-button>\n          </div>\n        </div>\n      </a-col>\n    </a-row>\n    <!-- 工厂名称抽屉 -->\n    <a-drawer\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"onClose\"\n      :visible=\"companyVisible\"\n      :width=\"300\"\n    >\n      <companyList :factoryId=\"factoryId\" @selectCompany=\"selectName\"></companyList>\n    </a-drawer>\n\n    <!-- 产品信息抽屉 -->\n    <a-drawer\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"closeDrawer\"\n      :visible=\"visible\"\n      :width=\"1080\"\n    >\n      <dispatchInfo @isDrawer=\"isDrawer\"></dispatchInfo>\n    </a-drawer>\n  </div>\n</template>\n\n<script>\nimport infiniteScroll from 'vue-infinite-scroll'\nimport { postJson, get } from '@/api/axios.js'\nimport api from '@/api/index.js'\nimport moment from 'moment'\nimport companyList from './child_lib/companyList'\nimport dispatchInfo from '@/views/product/pdm/goods/child_info/GoodsInfo'\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin'\nimport { mixinCurrentPage, mixinGetProductAbout } from '@/views/sale/mixin/minxinSale'\nexport default {\n  directives: { infiniteScroll },\n  mixins: [mixinBasic, mixinDrawer, mixinCurrentPage, mixinGetProductAbout],\n  components: {\n    companyList,\n    dispatchInfo\n  },\n  data() {\n    return {\n      factoryId: '',\n      dateFormat: 'YYYY-MM-DD',\n      busy: false,\n      companyVisible: false,\n      loadingCompony: false,\n      dataCompanySize: 0,\n      dataCompany: [],\n      factoryOrderDetailPostBean: {\n        currentPage: 1,\n        pageSize: 15,\n        cz: '',\n        spec: '',\n        startTime: '',\n        endTime: ''\n      },\n      companyName: '请选择工厂',\n      companyList: [],\n      companyNameList: [],\n      updateList: {\n        num: 0,\n        date: ''\n      },\n      submitLoading: false,\n      timer: '',\n      goodsViewInfo: {}\n    }\n  },\n  created() {\n    this.getListCompanySpringCz(this.specCurrentPage)\n    this.getListCompanySpringSpecs(this.czCurrentPage)\n  },\n  methods: {\n    moment,\n    // 按条件搜索渠道商订单列表\n    inputChange(val, type) {\n      this.dataCompany = []\n      const factoryOrderDetailPostBean = this.factoryOrderDetailPostBean\n      factoryOrderDetailPostBean.currentPage = 1\n      if (type === 'date') {\n        factoryOrderDetailPostBean.startTime = val[0]\n        factoryOrderDetailPostBean.endTime = val[1]\n      } else {\n        factoryOrderDetailPostBean[type] = val\n      }\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.getFactoryOrder()\n      }, 1000)\n    },\n    // 滚动滑动条加载订单\n    handleInfiniteOnLoad() {\n      const data = this.dataCompany\n      this.loadingCompony = true\n      this.busy = true\n      if (data.length >= this.dataCompanySize && this.dataCompanySize !== 0) {\n        this.busy = true\n        this.loadingCompony = false\n      } else {\n        this.getFactoryOrder()\n        this.busy = false\n        this.loadingCompony = false\n      }\n    },\n    // 移除订单\n    removeItem(id, index) {\n      var _this = this\n      this.$confirm({\n        title: '移除订单信息',\n        content: '您确定要移除这条订单消息吗?',\n        zIndex: 99999,\n        onOk() {\n          postJson(api.delFactoryOrder, { id: id }).then(res => {\n            if (res.data.code === 0) {\n              _this.$message.success('移除成功', 1)\n              _this.dataCompany.splice(index, 1)\n              _this.dataCompanySize--\n            }\n          })\n        },\n        onCancel() { }\n      })\n    },\n    // 打开抽屉\n    showDrawer() {\n      this.companyVisible = !this.companyVisible\n    },\n    // 关闭抽屉\n    onClose() {\n      this.companyVisible = false\n    },\n    // 分派订单到工厂\n    addCompany(id, index, type) {\n      const dataCompany = this.dataCompany\n      const companyList = this.companyList\n      const updateList = this.updateList\n      this.dataCompany[index].clickedVisible = false\n      const tar = companyList.filter(item => item.id === id)[0]\n      if (tar) {\n        if (type === 'addAll') {\n          tar.odNumber += dataCompany[index].odNumber\n          this.dataCompanySize--\n          dataCompany.splice(index, 1)\n        } else {\n          if (updateList.num >= dataCompany[index].odNumber) {\n            this.dataCompanySize--\n            dataCompany.splice(index, 1)\n          } else {\n            dataCompany[index].odNumber -= updateList.num\n          }\n          tar.odPromiseDueDate = updateList.date\n          tar.odNumber += updateList.num\n        }\n      } else {\n        if (type === 'addAll') {\n          companyList.push(dataCompany[index])\n          dataCompany.splice(index, 1)\n          this.dataCompanySize--\n        } else {\n          const target = dataCompany[index]\n          if (updateList.num >= target.odNumber) {\n            target.odPromiseDueDate = updateList.date\n            companyList.push(target)\n            dataCompany.splice(index, 1)\n            this.dataCompanySize--\n          } else {\n            const team = JSON.parse(JSON.stringify(target))\n            target.odNumber -= updateList.num\n            team.odNumber = updateList.num\n            team.odPromiseDueDate = updateList.date\n            companyList.push(team)\n          }\n        }\n      }\n    },\n    // 删除工厂中的订单\n    minusCompany(id, index) {\n      const dataCompany = this.dataCompany\n      const companyList = this.companyList\n      const target = dataCompany.filter(item => item.id === id)[0]\n      if (target) {\n        target.odNumber += companyList[index].odNumber\n      } else {\n        dataCompany.push(companyList[index])\n      }\n      companyList.splice(index, 1)\n    },\n    // 点击取消\n    handleClose() {\n      this.factoryOrderDetailPostBean.currentPage = 1\n      this.dataCompany = []\n      this.companyList.splice(0)\n      this.getFactoryOrder()\n    },\n    // 获取渠道商订单列表\n    getFactoryOrder() {\n      postJson(api.getFactoryOrder, this.factoryOrderDetailPostBean).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data.records\n          if (data.length === 0) {\n            this.$message.warning('暂时没有更多订单数据')\n            return\n          }\n          data.forEach(item => {\n            item.clickedVisible = false\n          })\n          this.dataCompanySize = Number(res.data.data.total)\n          this.dataCompany = this.dataCompany.concat(data)\n          this.factoryOrderDetailPostBean.currentPage += 1\n        }\n      })\n    },\n    // 设置选中的订单产品的数量和交期\n    updataDes(num, data, index) {\n      const updateList = this.updateList\n      updateList.num = num\n      updateList.date = data\n      this.dataCompany[index].clickedVisible = true\n    },\n    updateTime(dates, dateStrings) {\n      this.updateList.date = dateStrings\n    },\n    // 订单分派提交\n    submit() {\n      if (this.companyList.length === 0) {\n        return\n      }\n      if (this.factoryId === '') {\n        this.$message.warning('请选择工厂')\n        return\n      }\n      const addFactorySaleOrderPostBean = {\n        factoryId: this.factoryId,\n        list: []\n      }\n      const list = addFactorySaleOrderPostBean.list\n      const companyList = this.companyList\n      companyList.forEach(item => {\n        list.push(\n          {\n            id: item.id,\n            number: item.odNumber,\n            orderNumber: item.orderNumber,\n            odRequireDueDate: item.odPromiseDueDate\n          }\n        )\n      })\n      this.submitLoading = true\n      postJson(api.addFactoryOrder, addFactorySaleOrderPostBean).then(res => {\n        this.submitLoading = false\n        if (res.data.code === 0) {\n          this.$message.success('分派成功')\n          this.factoryOrderDetailPostBean.currentPage = 1\n          this.companyList = []\n          this.dataCompany = []\n          this.getFactoryOrder()\n        } else {\n          this.$message.error(res.data.msg)\n        }\n      })\n    },\n    // 替换显示工厂名字以及工厂id\n    selectName(data) {\n      this.factoryId = data.id\n      this.companyName = data.name\n      this.companyVisible = false\n    },\n    // 关闭抽屉\n    isDrawer(val) {\n      this.visible = val\n    },\n    // 查看产品详情\n    infoView(id) {\n      this.getViewInfo(id)\n      this.$store.commit('setPromptTitle', {\n        title: '产品信息',\n        isEdit: true\n      })\n    },\n    getViewInfo(params) {\n      get(api.goodsView + `?skuId=${params}`).then(res => {\n        if (res.data.code === 0) {\n          this.goodsViewInfo = res.data.data\n          this.visible = true\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    // 选择框的下拉加载\n    currentPageChange(e, type) {\n      const result = this.getCurrentPageChange(e)\n      if (result) {\n        if (type === 'cz') {\n          this.getListCompanySpringCz(this.czCurrentPage)\n        } else if (type === 'spec') {\n          this.getListCompanySpringSpecs(this.specCurrentPage)\n        }\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/dispach.less\";\n</style>\n"],"sourceRoot":"src/views/sale/dispatchOrder"}]}