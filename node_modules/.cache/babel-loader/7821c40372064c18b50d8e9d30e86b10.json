        format=\"YYYY-MM-DD HH:mm:ss\"\n                  @change=\"(dates, dateStrings)=>{inputChange(dateStrings,'date')}\"\n                />\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"8\">\n              <a-form-item label=\"材质\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n                <a-select\n                  style=\"width:160px\"\n                  showSearch\n                  :allowClear=\"true\"\n                  placeholder=\"请选择材质\"\n                  @change=\"(val)=>{inputChange(val,'cz')}\"\n                  @popupScroll=\"e=>currentPageChange(e,'cz')\"\n                  :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n                >\n                  <a-select-option v-for=\"item in textures\" :key=\"item\">{{item}}</a-select-option>\n                </a-select>\n              </a-form-item>\n            </a-col>\n          </a-row>\n        </div>\n        <div class=\"order-list\">\n          <div\n            class=\"order-list-item demo-infinite-container\"\n            v-infinite-scroll=\"handleInfiniteOnLoad\"\n            :infinite-scroll-disabled=\"busy\"\n            :infinite-scroll-distance=\"10\"\n          >\n            <a-list :dataSource=\"dataCompany\">\n              <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n                <div class=\"list-item\" @click=\"addCompany(item.id,item.channelSkuId,index)\">\n                  <div class=\"item-left\">\n                    <div class=\"item-left-top\">\n                      <a\n                        href=\"javascript:;\"\n                        @click=\"e=>infoView(e,item.channelSkuId)\"\n                      >{{item.name}} {{item.bom}}</a>\n                      <span>{{item.craft}}</span>\n                    </div>\n                    <ul class=\"item-left-desc\">\n                      <li>{{item.spec}}</li>\n                      <li>{{item.cz}}</li>\n                      <li>{{item.bmcl}}</li>\n                      <li>{{moment(item.odPromiseDueDate).format(dateFormat)}}</li>\n                      <li>{{item.number}}{{item.unit}}</li>\n                    </ul>\n                  </div>\n                  <div class=\"item-right\">\n                    <a-popover trigger=\"click\" v-model=\"item.clickedVisible\">\n                      <template slot=\"content\">\n                        <a-form-item class=\"mb\">\n                          <a-button type=\"danger\" @click=\"removeItem(item.id,index)\">移除</a-button>\n                        </a-form-item>\n                      </template>\n                      <a-icon\n                        type=\"ellipsis\"\n                        class=\"item-right-edit\"\n                        @click=\"e=>updataDes(e,index)\"\n                      />\n                    </a-popover>\n                    <a-button type=\"primary\" shape=\"circle\" icon=\"plus\"></a-button>\n                  </div>\n                </div>\n              </a-list-item>\n              <div v-if=\"loadingCompony && !busy\" class=\"demo-loading-container\">\n                <a-spin/>\n              </div>\n            </a-list>\n          </div>\n        </div>\n      </a-col>\n      <a-col :span=\"12\" class=\"dispatch-right\">\n        <div class=\"title\"></div>\n        <div class=\"order-list demo-infinite-container\">\n          <div class=\"list-item\" v-if=\"orderMerge.name\">\n            <div class=\"item-left\">\n              <div class=\"item-left-top\">\n                <a href=\"javascript:;\">{{orderMerge.name}} {{orderMerge.bom}}</a>\n                <span>{{orderMerge.craft}}</span>\n              </div>\n              <ul class=\"item-left-desc\">\n                <li>{{orderMerge.spec}}</li>\n                <li>{{orderMerge.cz}}</li>\n                <li>{{orderMerge.bmcl}}</li>\n                <li>{{moment(orderMerge.odPromiseDueDate).format(dateFormat)}}</li>\n                <li>{{orderMerge.number}}{{orderMerge.unit}}</li>\n              </ul>\n            </div>\n            <div class=\"item-right\">\n              <a-button\n                type=\"danger\"\n                shape=\"circle\"\n                icon=\"minus\"\n                style=\"background:#f5222d;color:#fff;\"\n                @click=\"minusCompany\"\n              ></a-button>\n            </div>\n          </div>\n          <div class=\"dispatch-right-bottom\">\n            <a-button @click=\"minusCompany\">取消</a-button>\n            <a-button type=\"primary\" @click=\"submit\" :loading=\"submitLoading\">生成工单</a-button>\n          </div>\n        </div>\n      </a-col>\n    </a-row>\n    <!-- 产品信息抽屉 -->\n    <a-drawer\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"closeDrawer\"\n      :visible=\"visible\"\n      :width=\"1080\"\n    >\n      <dispatchInfo @isDrawer=\"isDrawer\" :goodsViewInfo=\"goodsViewInfo\"></dispatchInfo>\n    </a-drawer>\n  </div>\n</template>\n\n<script>\nimport infiniteScroll from 'vue-infinite-scroll'\nimport { postJson, get } from '@/api/axios.js'\nimport api from '@/api/index.js'\nimport moment from 'moment'\nimport dispatchInfo from '@/views/product/pdm/goods/child_info/GoodsInfo'\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin'\nimport { mixinCurrentPage, mixinGetProductAbout } from '@/views/sale/mixin/minxinSale'\nexport default {\n  directives: { infiniteScroll },\n  mixins: [mixinBasic, mixinDrawer, mixinCurrentPage, mixinGetProductAbout],\n  components: {\n    dispatchInfo\n  },\n  data() {\n    return {\n      dateFormat: 'YYYY-MM-DD',\n      busy: false,\n      loadingCompony: false,\n      dataCompanySize: 0,\n      dataCompany: [],\n      orderMerge: {},\n      currentPage: 1,\n      pageSize: 15,\n      factoryOrderDetailPostBean: {\n        cz: '',\n        spec: '',\n        startTime: '',\n        endTime: ''\n      },\n      companyList: [],\n      submitLoading: false,\n      timer: '',\n      goodsViewInfo: {}\n    }\n  },\n  created() {\n    this.getFactoryPolicyOrderCz(this.specCurrentPage)\n    this.getFactoryPolicyOrderSpec(this.czCurrentPage)\n  },\n  methods: {\n    moment,\n    // 按条件搜索渠道商订单列表\n    inputChange(val, type) {\n      this.dataCompany = []\n      const factoryOrderDetailPostBean = this.factoryOrderDetailPostBean\n      factoryOrderDetailPostBean.currentPage = 1\n      if (type === 'date') {\n        factoryOrderDetailPostBean.startTime = val[0]\n        factoryOrderDetailPostBean.endTime = val[1]\n      } else {\n        factoryOrderDetailPostBean[type] = val\n      }\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.getFactoryOrder()\n      }, 1000)\n    },\n    // 滚动滑动条加载订单\n    handleInfiniteOnLoad() {\n      const data = this.dataCompany\n      this.loadingCompony = true\n      this.busy = true\n      if (data.length >= this.dataCompanySize && this.dataCompanySize !== 0) {\n        this.busy = true\n        this.loadingCompony = false\n      } else {\n        this.getFactoryOrder(this.currentPage)\n        this.busy = false\n        this.loadingCompony = false\n      }\n    },\n    // 移除订单\n    removeItem(id, index) {\n      const _this = this\n      this.$confirm({\n        title: '移除订单信息',\n        content: '您确定要移除这条订单消息吗?',\n        zIndex: 99999,\n        onOk() {\n          postJson(api.delFactoryPolicyOrder, { id }).then(res => {\n            if (res.data.code === 0) {\n              _this.$message.success('订单移除成功')\n              _this.dataCompany.splice(index, 1)\n              _this.dataCompanySize--\n            } else {\n              _this.$message.error(res.data.msg)\n            }\n          })\n        },\n        onCancel() { }\n      })\n    },\n    // 分派订单到车间\n    addCompany(id, channelSkuId, index) {\n      const dataCompany = this.dataCompany\n      const companyList = this.companyList\n      const item = dataCompany[index]\n      dataCompany[index].clickedVisible = false\n      if (item.bom === null || item.craft === null) {\n        const _this = this\n        this.$confirm({\n          title: '确认制造工艺和BOM?',\n          content: '请先选择制造工艺和BOM',\n          onOk() {\n            _this.getViewInfo(channelSkuId)\n            _this.$store.commit('setPromptTitle', {\n              title: '产品信息',\n              isEdit: true\n            })\n          }\n        })\n        return\n      }\n      if (companyList.length === 0) {\n        this.orderMerge = { ...item }\n        companyList.push(item)\n        dataCompany.splice(index, 1)\n      } else {\n        if (item.spec !== companyList[0].spec || item.cz !== companyList[0].cz) {\n          this.$message.warning('不同产品不能合并生成工单。')\n          return\n        }\n        if (item.bom !== companyList[0].bom || item.craft !== companyList[0].craft) {\n          this.$message.warning('不同制造工艺和BOM不能合并生成工单。')\n          return\n        }\n        companyList.push(item)\n        this.orderMerge.number += item.number\n        dataCompany.splice(index, 1)\n      }\n    },\n    // 删除工厂中的订单\n    minusCompany() {\n      this.companyList.splice(0)\n      const currentPage = 1\n      this.dataCompany = []\n      this.orderMerge = {}\n      this.getFactoryOrder(currentPage)\n    },\n    // 获取工厂订单列表\n    getFactoryOrder(currentPage = 1, pageSize = 15) {\n      const data = this.factoryOrderDetailPostBean\n      data.currentPage = currentPage\n      data.pageSize = pageSize\n      postJson(api.getFactoryPolicyOrder, data).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data.records\n          this.dataCompanySize = Number(res.data.data.total)\n          if (data.length) {\n            data.forEach(item => {\n              item.clickedVisible = false\n            })\n            this.dataCompany = this.dataCompany.concat(data)\n          }\n          this.currentPage += 1\n        }\n      })\n    },\n    updataDes(e, index) {\n      window.event ? window.event.cancelBubble = true : e.stopPropagation()\n      this.dataCompany[index].clickedVisible = true\n    },\n    // 订单分派提交\n    submit() {\n      if (this.companyList.length === 0) {\n        return\n      }\n      this.submitLoading = true\n      const list = []\n      this.companyList.forEach(item => {\n        list.push({\n          id: item.id,\n          number: item.number\n        })\n      })\n      postJson(api.putFactoryPolicyOrderAdd, {\n        list: [\n          {\n            list: [...list],\n            odRequireDueDate: this.orderMerge.odRequireDueDate,\n            totalNumber: this.orderMerge.number\n          }\n        ],\n        remarks: ''\n      }).then(res => {\n        if (res.data.code === 0) {\n          this.$message.success('分单成功')\n          this.companyList = []\n          this.orderMerge = {}\n          this.dataCompany = []\n          this.getFactoryOrder()\n        } else {\n          this.$message.error(res.data.msg)\n        }\n        this.submitLoading = false\n      })\n    },\n    // 关闭抽屉\n    isDrawer(val) {\n      this.visible = val\n    },\n    // 查看产品详情\n    infoView(e, id) {\n      window.event ? window.event.cancelBubble = true : e.stopPropagation()\n      this.getViewInfo(id)\n      this.$store.commit('setPromptTitle', {\n        title: '产品信息',\n        isEdit: true\n      })\n    },\n    getViewInfo(params) {\n      get(api.goodsView + `?skuId=${params}`).then(res => {\n        if (res.data.code === 0) {\n          this.goodsViewInfo = res.data.data\n          this.visible = true\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    // 选择框的下拉加载\n    currentPageChange(e, type) {\n      const result = this.getCurrentPageChange(e)\n      if (result) {\n        if (type === 'cz') {\n          this.getFactoryPolicyOrderCz(this.czCurrentPage)\n        } else if (type === 'spec') {\n          this.getFactoryPolicyOrderSpec(this.specCurrentPage)\n        }\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/dispach.less\";\n</style>\n"],"sourceRoot":"src/views/factorySale/dispatchOrder"}]}