      <a-input placeholder=\"请输入客户名称\"/>\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"6\">\n            <a-form-item label=\"分类编码\" :label-col=\"{ span: 8 }\" :wrapper-col=\"{ span: 16 }\">\n              <a-input placeholder=\"请输入客户名称\"/>\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"6\">\n            <a-form-item>\n              <a-button type=\"primary\">查询</a-button>\n              <a-button class=\"category-select-reset\">重置</a-button>\n            </a-form-item>\n          </a-col>\n        </a-row>\n      </a-form>\n    </div>-->\n    <!-- 产品分类 -->\n    <div class=\"category-tab\">\n      <a-table\n        :columns=\"columns\"\n        :dataSource=\"data\"\n        :loading=\"loading\"\n        :pagination=\"pagination\"\n        @change=\"onChangePage\"\n        :rowKey=\"record => record.id\"\n        childrenColumnName=\"childrens\"\n      >\n        <template slot=\"name\" slot-scope=\"text, record\">\n          <a @click=\"onHandleDetail(record)\" href=\"javascript:;\">{{ text }}</a>\n        </template>\n      </a-table>\n    </div>\n  </div>\n</template>\n<script>\nimport CateGoryInfo from './categoryInfo'\nimport { getCategoryTree, getCategoryDetail } from './getCategory.js'\nimport { setTimeout, clearTimeout } from 'timers'\n\nconst columns = [\n  {\n    title: '客户简称',\n    dataIndex: 'name',\n    scopedSlots: { customRender: 'name' }\n  },\n  { title: '客户编码', dataIndex: 'code' },\n  { title: '备注', dataIndex: 'remark' }\n]\n\nexport default {\n  name: 'Category',\n  data() {\n    return {\n      visible: false,\n      isEdit: false,\n      data: [],\n      columns,\n      timer: null,\n      loading: false,\n      pagination: {\n        total: 0,\n        defaultPageSize: 15,\n        defaultCurrent: 1,\n        showQuickJumper: true,\n        hideOnSinglePage: true,\n        showSizeChanger: true,\n        pageSizeOptions: ['10', '15', '30', '50', '100']\n      },\n      // 产品分类详情\n      viewCategoryInfo: null,\n      isPermission: true\n    }\n  },\n  created() {\n    // 初始化tab数据\n    getCategoryTree.call(this, 1)\n  },\n  beforeDestroy() {\n    clearTimeout(this.timer)\n  },\n  methods: {\n    // 详情\n    onHandleDetail(record) {\n      console.log(!isNaN(Number(record.id)))\n      if (!isNaN(Number(record.id)) && this.isPermission) {\n        console.log(record.id)\n        getCategoryDetail.call(this, record.id)\n        // console.log(this.viewCategoryInfo)\n        this.visible = true\n        // 编辑按钮显示\n        this.isEdit = true\n        return false\n      }\n      this.$message.error('请选择下级分类')\n    },\n    // 新增弹出\n    showDrawer() {\n      this.visible = true\n      // 取消编辑显示\n      this.isEdit = false\n    },\n    // $emit改变isvisible\n    isVisible(isVisible) {\n      this.visible = isVisible\n      // 每次关闭弹出层主动初始化数据\n      this.timer = setTimeout(() => {\n        getCategoryTree.call(this, 1)\n      }, 200)\n    },\n    // 分页管理\n    onChangePage(pagination, filters, sorter) {\n      console.log(pagination.current)\n      getCategoryTree.call(this, pagination.current, pagination.pageSize)\n    },\n    onClose() {\n      this.visible = false\n    },\n    // 权限\n    controlPermission(val) {\n      val.forEach(item => {\n        // console.log(item.btnOptType)\n        switch (item.btnOptType) {\n          case 'LIST':\n            // 客户搜索\n            console.log('LIST')\n            this.isPermission = true\n            break\n          case 'IMPORT':\n            // 导入\n            console.log('IMPORT')\n            this.isPermission = true\n            break\n          case 'EXPORT':\n            // 导出\n            console.log('EXPORT')\n            this.isPermission = true\n            break\n          case 'DELETES':\n            // 删除\n            console.log('DELETES')\n            this.isPermission = true\n            break\n          case 'ADD':\n            // 新建\n            console.log('ADD')\n            this.isPermission = true\n            break\n          case 'DETAIL':\n            // 详情\n            console.log('DETAIL')\n            this.isPermission = true\n            break\n          default:\n            console.log('other')\n            this.isPermission = false\n        }\n      })\n    }\n  },\n  computed: {\n    getNavPermission() {\n      return this.$store.getters.getNavPermission\n    }\n  },\n  watch: {\n    getNavPermission(val) {\n      // console.log(val)\n      this.controlPermission(val)\n    }\n  },\n  components: {\n    CateGoryInfo\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n@import '~styles/variable.less';\n\n.category {\n  .category-add {\n    margin-bottom: 15px;\n  }\n  .category-remove {\n    margin: 0 10px;\n  }\n  .category-select-reset {\n    margin: 0 10px;\n  }\n  .category-select {\n    margin: 16px 0;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/product/category"}]}