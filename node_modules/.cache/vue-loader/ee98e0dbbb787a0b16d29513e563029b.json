;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8OA","file":"delivery.vue","sourceRoot":"src/views/sale/delivery","sourcesContent":["<template>\n  <div class=\"dispatch\">\n    <a-row>\n      <a-col :span=\"12\" class=\"dispatch-left\">\n        <a-row class=\"search\">\n          <a-col :span=\"8\">\n            <a-form-item label=\"规格\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-select\n                style=\"width:160px\"\n                showSearch\n                :allowClear=\"true\"\n                placeholder=\"请选择规格\"\n                @change=\"(val)=>{inputChange(val,'spec')}\"\n                @popupScroll=\"e=>currentPageChange(e,'spec')\"\n                :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n              >\n                <a-select-option v-for=\"(item,index) in odSpecList\" :key=\"index\">{{item}}</a-select-option>\n              </a-select>\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"8\">\n            <a-form-item label=\"交期\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-range-picker\n                style=\"width:160px\"\n                format=\"YYYY-MM-DD HH:mm:ss\"\n                @change=\"(dates, dateStrings)=>{inputChange(dateStrings,'date')}\"\n              />\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"8\">\n            <a-form-item label=\"材质\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-select\n                style=\"width:160px\"\n                showSearch\n                :allowClear=\"true\"\n                placeholder=\"请选择材质\"\n                @change=\"(val)=>{inputChange(val,'cz')}\"\n                @popupScroll=\"e=>currentPageChange(e,'cz')\"\n                :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n              >\n                <a-select-option v-for=\"(item,index) in textures\" :key=\"index\">{{item}}</a-select-option>\n              </a-select>\n            </a-form-item>\n          </a-col>\n        </a-row>\n        <div\n          class=\"order-list order-list-item demo-infinite-container\"\n          v-infinite-scroll=\"handleInfiniteOnLoad\"\n          :infinite-scroll-disabled=\"busy\"\n          :infinite-scroll-distance=\"10\"\n        >\n          <a-list :dataSource=\"dataCompany\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n              <div class=\"list-item\" @click=\"addCompany(item.id,index)\">\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a href=\"javascript:;\">{{item.name}}</a>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.spec}}</li>\n                    <li>{{item.cz}}</li>\n                    <li>{{item.bmcl}}</li>\n                    <li>{{item.odNumber}}{{item.unit}}</li>\n                  </ul>\n                </div>\n                <div class=\"item-right\">\n                  <a-popover trigger=\"click\" v-model=\"item.clickedVisible\">\n                    <template slot=\"content\">\n                      <a-form-item label=\"紧急程度:\" :label-col=\"{ span: 12}\" :wrapper-col=\"{span:12}\">\n                        <a-switch/>\n                      </a-form-item>\n                      <a-form-item class=\"mb\">\n                        <a-button type=\"danger\" @click=\"removeItem(item.id)\">移除</a-button>\n                      </a-form-item>\n                    </template>\n                    <a-icon type=\"ellipsis\" class=\"item-right-edit\" @click=\"updataDes(index)\"/>\n                  </a-popover>\n                </div>\n              </div>\n            </a-list-item>\n            <div v-if=\"loadingCompony && !busy\" class=\"demo-loading-container\">\n              <a-spin/>\n            </div>\n          </a-list>\n        </div>\n      </a-col>\n      <a-col :span=\"12\" class=\"dispatch-right procurement\">\n        <div class=\"title\"></div>\n        <div class=\"order-list demo-infinite-container\">\n          <a-list :dataSource=\"companyList\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n              <div class=\"list-item\">\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a href=\"javascript:;\">{{item.name}} {{item.bom}}</a>\n                    <span>{{item.craft}}</span>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.spec}}</li>\n                    <li>{{item.cz}}</li>\n                    <li>{{item.bmcl}}</li>\n                  </ul>\n                </div>\n                <div class=\"item-right\">\n                  <a-input-number :min=\"0\" :value=\"100\"/>\n                </div>\n              </div>\n            </a-list-item>\n          </a-list>\n          <div style=\"height:40px\"></div>\n          <div class=\"dispatch-right-bottom\">\n            <a-button>取消</a-button>\n            <a-button type=\"primary\" @click=\"submit\" :loading=\"submitLoading\">发货</a-button>\n          </div>\n        </div>\n      </a-col>\n    </a-row>\n  </div>\n</template>\n\n<script>\nimport infiniteScroll from 'vue-infinite-scroll'\nimport { postJson } from '@/api/axios.js'\nimport api from '@/api/index.js'\nimport { mixinBasic } from '@/components/tools/mixin/mixin'\nimport { mixinCurrentPage, mixinGetProductAbout } from '@/views/sale/mixin/minxinSale'\nexport default {\n  directives: { infiniteScroll },\n  mixins: [mixinBasic, mixinCurrentPage, mixinGetProductAbout],\n  data() {\n    return {\n      busy: false,\n      companyVisible: false,\n      loadingCompony: false,\n      dataCompanySize: 0,\n      dataCompany: [],\n      factoryOrderDetailPostBean: {\n        currentPage: 1,\n        pageSize: 15,\n        cz: '',\n        spec: '',\n        startTime: '',\n        endTime: ''\n      },\n      companyList: [],\n      submitLoading: false\n    }\n  },\n  created() {\n    this.getListCompanySpringCz(this.specCurrentPage)\n    this.getListCompanySpringSpecs(this.czCurrentPage)\n  },\n  methods: {\n    inputChange(val, type) {\n      this.dataCompany = []\n      const factoryOrderDetailPostBean = this.factoryOrderDetailPostBean\n      factoryOrderDetailPostBean.currentPage = 1\n      switch (type) {\n        case 'spec':\n          factoryOrderDetailPostBean.spec = this.specification[val]\n          break\n        case 'cz':\n          factoryOrderDetailPostBean.cz = this.textures[val]\n          break\n        case 'date':\n          factoryOrderDetailPostBean.startTime = val[0]\n          factoryOrderDetailPostBean.endTime = val[1]\n          break\n      }\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.getFactoryOrder()\n      }, 1000)\n    },\n    handleInfiniteOnLoad() {\n      const data = this.dataCompany\n      this.loadingCompony = true\n      this.busy = true\n      if (data.length >= this.dataCompanySize && this.dataCompanySize !== 0) {\n        this.busy = true\n        this.loadingCompony = false\n      } else {\n        this.getFactoryOrder()\n        this.busy = false\n        this.loadingCompony = false\n      }\n    },\n    removeItem(id) {\n      // var _this = this\n      this.$confirm({\n        title: '移除订单信息',\n        content: '您确定要移除这条订单消息吗?',\n        zIndex: 99999,\n        onOk() {\n        },\n        onCancel() { }\n      })\n    },\n    addCompany(id, index) {\n    },\n    minusCompany(id, index) {\n    },\n    getFactoryOrder() {\n      postJson(api.getFactoryOrder, this.factoryOrderDetailPostBean).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data.records\n          data.forEach(item => {\n            item.clickedVisible = false\n          })\n          this.dataCompanySize = Number(res.data.data.total)\n          this.dataCompany = this.dataCompany.concat(data)\n          this.factoryOrderDetailPostBean.currentPage += 1\n        }\n      })\n    },\n    updataDes(index) {\n      this.dataCompany[index].clickedVisible = true\n    },\n    submit() {\n    },\n    // 选择框的下拉加载\n    currentPageChange(e, type) {\n      const result = this.getCurrentPageChange(e)\n      if (result) {\n        if (type === 'cz') {\n          this.getListCompanySpringCz(this.czCurrentPage)\n        } else if (type === 'spec') {\n          this.getListCompanySpringSpecs(this.specCurrentPage)\n        }\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/dispach.less\";\n</style>\n"]}]}