 getCategoryTree.call(this, 1)\n  },\n  beforeDestroy() {\n    clearTimeout(this.timer)\n  },\n  methods: {\n    // 详情\n    onHandleDetail(record) {\n      console.log(!isNaN(Number(record.id)))\n      if (!isNaN(Number(record.id))) {\n        console.log(record.id)\n        getCategoryDetail.call(this, record.id)\n        // console.log(this.viewCategoryInfo)\n        this.visible = true\n        this.title = '产品信息详情'\n        // 编辑按钮显示\n        this.isEdit = true\n        return false\n      }\n      this.$message.error('请选择下级分类')\n    },\n    // 新增弹出\n    showDrawer() {\n      this.visible = true\n      // 取消编辑显示\n      this.isEdit = false\n    },\n    // $emit改变isvisible\n    isVisible(isVisible) {\n      this.visible = isVisible\n      // 每次关闭弹出层主动初始化数据\n      this.timer = setTimeout(() => {\n        getCategoryTree.call(this, 1)\n      }, 200)\n    },\n    // 分页管理\n    onChangePage(pagination, filters, sorter) {\n      console.log(pagination.current)\n      getCategoryTree.call(this, pagination.current)\n    },\n    onClose() {\n      this.visible = false\n    }\n  },\n  components: {\n    CateGoryInfo\n  }\n}\n",{"version":3,"sources":["Category.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Category.vue","sourceRoot":"src/views/product/category","sourcesContent":["<template>\n  <div class=\"category\">\n    <!-- 产品分类选择区 -->\n    <div class=\"category-add\">\n      <a-button type=\"primary\" @click=\"showDrawer\">\n        <a-icon type=\"plus\"/>新建\n      </a-button>\n      <!-- 弹出层 -->\n      <a-drawer\n        destroyOnClose\n        :title=\"title\"\n        placement=\"right\"\n        :closable=\"false\"\n        @close=\"onClose\"\n        :visible=\"visible\"\n        :width=\"1040\"\n        :wrapStyle=\"{height: 'calc(100% - 108px)',overflow: 'auto',paddingBottom: '108px'}\"\n      >\n        <CateGoryInfo @isVisible=\"isVisible\" :viewCategoryInfo=\"viewCategoryInfo\" :isEdit=\"isEdit\"></CateGoryInfo>\n      </a-drawer>\n      <!-- <a-button type=\"danger\" class=\"category-remove\" @click=\"onDeleteProCategory\">\n        <a-icon type=\"delete\"/>删除\n      </a-button>-->\n    </div>\n    <!-- 条件筛选 -->\n    <div class=\"category-select\">\n      <a-form layout=\"inline\">\n        <a-row :gutter=\"16\">\n          <a-col :span=\"6\">\n            <a-form-item label=\"分类名称\" :label-col=\"{ span: 8 }\" :wrapper-col=\"{ span: 16 }\">\n              <a-input placeholder=\"请输入客户名称\"/>\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"6\">\n            <a-form-item label=\"分类编码\" :label-col=\"{ span: 8 }\" :wrapper-col=\"{ span: 16 }\">\n              <a-input placeholder=\"请输入客户名称\"/>\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"6\">\n            <a-form-item>\n              <a-button type=\"primary\">查询</a-button>\n              <a-button class=\"category-select-reset\">重置</a-button>\n            </a-form-item>\n          </a-col>\n        </a-row>\n      </a-form>\n    </div>\n    <!-- 产品分类 -->\n    <div class=\"category-tab\">\n      <a-table\n        :columns=\"columns\"\n        :dataSource=\"data\"\n        :loading=\"loading\"\n        :pagination=\"pagination\"\n        @change=\"onChangePage\"\n        :rowKey=\"record => record.id\"\n        childrenColumnName=\"childrens\"\n      >\n        <template slot=\"name\" slot-scope=\"text, record\">\n          <a @click=\"onHandleDetail(record)\" href=\"javascript:;\">{{ text }}</a>\n        </template>\n      </a-table>\n    </div>\n  </div>\n</template>\n<script>\nimport CateGoryInfo from './categoryInfo'\nimport { getCategoryTree, getCategoryDetail } from './getCategory.js'\nimport { setTimeout, clearTimeout } from 'timers'\n\nconst columns = [\n  {\n    title: '客户简称',\n    dataIndex: 'name',\n    scopedSlots: { customRender: 'name' }\n  },\n  { title: '客户编码', dataIndex: 'code' },\n  { title: '备注', dataIndex: 'remark' }\n]\n\nexport default {\n  name: 'Category',\n  data() {\n    return {\n      title: '新建分类信息',\n      visible: false,\n      isEdit: false,\n      data: [],\n      columns,\n      timer: null,\n      loading: false,\n      pagination: {\n        total: 27,\n        pageSize: 5,\n        defaultCurrent: 1\n      },\n      // 产品分类详情\n      viewCategoryInfo: null\n    }\n  },\n  created() {\n    // 初始化tab数据\n    getCategoryTree.call(this, 1)\n  },\n  beforeDestroy() {\n    clearTimeout(this.timer)\n  },\n  methods: {\n    // 详情\n    onHandleDetail(record) {\n      console.log(!isNaN(Number(record.id)))\n      if (!isNaN(Number(record.id))) {\n        console.log(record.id)\n        getCategoryDetail.call(this, record.id)\n        // console.log(this.viewCategoryInfo)\n        this.visible = true\n        this.title = '产品信息详情'\n        // 编辑按钮显示\n        this.isEdit = true\n        return false\n      }\n      this.$message.error('请选择下级分类')\n    },\n    // 新增弹出\n    showDrawer() {\n      this.visible = true\n      // 取消编辑显示\n      this.isEdit = false\n    },\n    // $emit改变isvisible\n    isVisible(isVisible) {\n      this.visible = isVisible\n      // 每次关闭弹出层主动初始化数据\n      this.timer = setTimeout(() => {\n        getCategoryTree.call(this, 1)\n      }, 200)\n    },\n    // 分页管理\n    onChangePage(pagination, filters, sorter) {\n      console.log(pagination.current)\n      getCategoryTree.call(this, pagination.current)\n    },\n    onClose() {\n      this.visible = false\n    }\n  },\n  components: {\n    CateGoryInfo\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.category {\n  .category-remove {\n    margin: 0 10px;\n  }\n  .category-select-reset {\n    margin: 0 10px;\n  }\n  .category-select {\n    margin: 16px 0;\n  }\n}\n</style>\n"]}]}