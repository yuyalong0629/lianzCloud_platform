eturn {\n      editTitle: '编辑'\n    }\n  },\n  methods: {\n    callback(key) {\n    },\n    onHandlePromptEdit() {\n      if (this.editTitle === '编辑') {\n        this.editTitle = '取消'\n        Bus.$emit('isDisabled', false)\n        return\n      }\n      Bus.$emit('isDisabled', true)\n      this.editTitle = '编辑'\n    },\n    onHandlePromptDelete() {\n      this.$confirm({\n        title: '删除',\n        content: '您确认要删除？',\n        okText: '删除',\n        okType: 'danger',\n        cancelText: '取消',\n        onOk() {\n          Bus.$emit('isDelete', false)\n        }\n      })\n    }\n  },\n  computed: {\n    getPromptTitle() {\n      return this.$store.state.promptTitle\n    },\n    getDetailPermission() {\n      return this.$store.state.detailPermission\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.prompt-title {\n  // border-bottom: 1px solid #ebedf0;\n  // min-height: 40px;\n  margin-bottom: 50px;\n  .prompt-wrapper {\n    line-height: 45px;\n    .prompt-edit {\n      margin: 0 12px;\n      cursor: pointer;\n      .eidt-icon {\n        color: green;\n        margin-right: 4px;\n      }\n    }\n    .prompt-delete {\n      cursor: pointer;\n      .delete-icon {\n        color: #f00;\n        margin-right: 4px;\n      }\n    }\n  }\n}\n</style>\n"]}]}