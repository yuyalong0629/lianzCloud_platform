ize = this.pagination.pageSize\n      postJson(api.warehousingList, data).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data.records\n          data.forEach((item, index) => {\n            item['key'] = index\n          })\n          this.pagination.total = Number(res.data.data.total)\n          this.data = data\n        }\n        this.loading = false\n      })\n    }\n  }\n}\n",{"version":3,"sources":["putOrder.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"putOrder.vue","sourceRoot":"src/views/warehouse/GRN","sourcesContent":["<template>\n  <div>\n    <a-row>\n      <a-col :span=\"24\" class=\"purchase-right\">\n        <a-form layout=\"horizontal\" style=\"margin-bottom:20px\">\n          <a-row>\n            <a-col :span=\"5\">\n              <a-form-item :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\" label=\"入库编号\">\n                <a-input\n                  v-model=\"workshopOrdersPostBean.applyCode\"\n                  class=\"inputWidth\"\n                  placeholder=\"请输入入库编号\"\n                  @change=\"e=>inputChange(e.target.value,'applyCode')\"\n                />\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"5\">\n              <a-form-item :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\" label=\"产品名称\">\n                <a-input\n                  v-model=\"workshopOrdersPostBean.name\"\n                  class=\"inputWidth\"\n                  placeholder=\"请输入产品名称\"\n                  @change=\"e=>inputChange(e.target.value,'name')\"\n                />\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"5\">\n              <a-form-item :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\" label=\"紧急程度\">\n                <a-select\n                  class=\"inputWidth\"\n                  :allowClear=\"true\"\n                  v-model=\"workshopOrdersPostBean.requireLevel\"\n                  placeholder=\"请选择紧急程度\"\n                  @change=\"(val)=>{inputChange(val,'requireLevel')}\"\n                >\n                  <a-select-option value=\"LEVEL0\">正常</a-select-option>\n                  <a-select-option value=\"LEVEL1\">紧急</a-select-option>\n                </a-select>\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"5\">\n              <a-form-item :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\" label=\"状态\">\n                <a-select\n                  class=\"inputWidth\"\n                  :allowClear=\"true\"\n                  v-model=\"workshopOrdersPostBean.status\"\n                  placeholder=\"请选择状态\"\n                  @change=\"(val)=>{inputChange(val,'status')}\"\n                >\n                  <a-select-option v-for=\"item in menuStatus\" :key=\"item.key\">{{item.value}}</a-select-option>\n                </a-select>\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"3\">\n              <a-form-item>\n                <span style=\"cursor: pointer;\" @click=\"searchReset\">重置</span>\n              </a-form-item>\n            </a-col>\n          </a-row>\n        </a-form>\n        <a-table\n          :columns=\"columns\"\n          :dataSource=\"data\"\n          @change=\"onChangePage\"\n          :pagination=\"pagination\"\n          :rowSelection=\"rowSelection\"\n          :loading=\"loading\"\n          :scroll=\"{ y: '70vh'}\"\n        >\n          <template\n            v-for=\"(col,index) in ['applyCode','code']\"\n            :slot=\"col\"\n            slot-scope=\"text,record\"\n          >\n            <a href=\"javascript:;\" :key=\"index\" @click=\"showDrawer(col,record)\">{{record[col]}}</a>\n          </template>\n          <template slot=\"instorageType\" slot-scope=\"text\">\n            <span v-show=\"text === 'INDEPENDENT'\">工厂自主申请</span>\n            <span v-show=\"text === 'PRODUCTION'\">工厂生产单完成入库申请</span>\n            <span v-show=\"text === 'UPPLIER'\">材料商自主申请</span>\n            <span v-show=\"text === 'VENDOR'\">材料商销售单入库申请</span>\n          </template>\n          <template slot=\"requireLevel\" slot-scope=\"text\">{{text==='LEVEL0'?'正常':'紧急'}}</template>\n          <!-- 状态 0.等待接单1.已接单 2.生产中 3.已完成 4.已取消 5已拒单 -->\n          <template slot=\"status\" slot-scope=\"text\">{{text | putStorageStatus}}</template>\n        </a-table>\n      </a-col>\n    </a-row>\n    <a-drawer\n      :destroyOnClose=\"true\"\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"closeDrawer\"\n      :visible=\"visible\"\n      :width=\"1040\"\n      :wrapStyle=\"{height: 'calc(100% - 108px)',overflow: 'auto',paddingBottom: '108px'}\"\n    >\n      <purchaseInfo @isDrawer=\"isDrawer\" :purchaseId=\"purchaseId\"/>\n    </a-drawer>\n  </div>\n</template>\n\n<script>\nimport api from '@/api/index'\nimport { postJson } from '@/api/axios'\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin'\nimport { puStorageStatus } from '@/views/sale/mixin/menu'\nimport { statusChange } from '@/views/sale/mixin/minxinSale'\nimport { columns } from './base_lib/columns'\nimport purchaseInfo from './child_info/purchaseInfo'\nexport default {\n  mixins: [mixinBasic, mixinDrawer, statusChange],\n  components: {\n    purchaseInfo\n  },\n  data() {\n    return {\n      columns: columns,\n      menuStatus: puStorageStatus,\n      data: [],\n      selectedRowKeys: [],\n      pagination: {\n        total: 0,\n        pageSize: 15,\n        defaultCurrent: 1,\n        showQuickJumper: true,\n        showSizeChanger: true,\n        pageSizeOptions: ['10', '15', '30', '50', '100']\n      },\n      workshopOrdersPostBean: {},\n      purchaseId: ''\n    }\n  },\n  created() {\n    this.getFactoryPolicyOrderList()\n  },\n  computed: {\n    rowSelection() {\n      const { selectedRowKeys } = this\n      return {\n        selectedRowKeys,\n        onChange: this.onChangeTab,\n        hideDefaultSelections: false\n      }\n    }\n  },\n  methods: {\n    // 重置搜索条件\n    searchReset() {\n      const workshopOrdersPostBean = this.workshopOrdersPostBean\n      for (const index in workshopOrdersPostBean) {\n        delete workshopOrdersPostBean[index]\n      }\n      this.getFactoryPolicyOrderList()\n    },\n    // 搜索\n    inputChange(val, type) {\n      this.workshopOrdersPostBean.currentPage = 1\n      this.workshopOrdersPostBean[type] = val\n      // 输入框防抖\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.getFactoryPolicyOrderList()\n      }, 500)\n    },\n    // 翻页\n    onChangePage(pagination, filters, sorter) {\n      this.pagination.pageSize = pagination.pageSize\n      this.getFactoryPolicyOrderList(pagination.current)\n    },\n    // 查看详情\n    showDrawer(type, record) {\n      switch (type) {\n        case 'applyCode':\n          this.visible = true\n          this.purchaseId = record.id\n          this.$store.commit('setPromptTitle', {\n            title: '采购详情',\n            isEdit: false\n          })\n          break\n      }\n    },\n    isDrawer(val) {\n      this.visible = val\n      this.getFactoryPolicyOrderList()\n    },\n    // 获取入库列表\n    getFactoryPolicyOrderList(currentPage = 1) {\n      this.loading = true\n      const data = this.workshopOrdersPostBean\n      data.currentPage = currentPage\n      data.pageSize = this.pagination.pageSize\n      postJson(api.warehousingList, data).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data.records\n          data.forEach((item, index) => {\n            item['key'] = index\n          })\n          this.pagination.total = Number(res.data.data.total)\n          this.data = data\n        }\n        this.loading = false\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.step-dot {\n  position: relative;\n  display: inline-block;\n  width: 10px;\n  height: 10px;\n  border-radius: 50%;\n  background-color: #eee;\n  margin: 4px;\n  &:not(:last-child)::after {\n    position: absolute;\n    content: \"\";\n    display: inline-block;\n    width: 8px;\n    height: 4px;\n    background-color: #eee;\n    right: -8px;\n    top: 3px;\n  }\n  &.finish {\n    background-color: #69b412;\n  }\n  &.error {\n    background-color: #fa5555;\n  }\n  &.process {\n    background-color: #f0bb2e;\n  }\n}\n</style>\n"]}]}