     get(api.getSelectProcessCout).then(res => {\n        if (res.data.code === 0) {\n          this.ApproveCount = res.data.data.ApproveCount\n          this.ReadCount = res.data.data.ReadCount\n        }\n      })\n    }\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.approval {\n  &-header {\n    position: fixed;\n    z-index: 1;\n    width: 100%;\n    padding: 0;\n    background: #fff;\n    box-shadow: 0 1px 7px 1px rgba(202, 202, 202, 0.5);\n    & .pd {\n      padding: 0 30px;\n    }\n  }\n  &-content {\n    background: #fff;\n    padding: 24px;\n    min-height: 92vh;\n    box-shadow: 0 1px 5px 1px rgba(175, 175, 175, 0.35);\n    border-radius: 0px 0px 4px 4px;\n  }\n  .message-member {\n    display: block;\n    position: absolute;\n    width: 50px;\n    height: 50px;\n    border-radius: 50%;\n    background-color: #5077d1;\n    font-size: 14px;\n    color: #fff;\n    text-align: center;\n    line-height: 50px;\n    top: 50%;\n  }\n}\n</style>\n"]}]}