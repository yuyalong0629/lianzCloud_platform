ACA;AACA;AACA","file":"Catattr.vue","sourceRoot":"src/views/product/catattr","sourcesContent":["<template>\n  <div class=\"catattr\">\n    <div class=\"catattr-setup\">\n      <a-button type=\"primary\" @click=\"showDrawer\">\n        <a-icon type=\"plus\"/>新建\n      </a-button>\n    </div>\n    <!-- 产品属性 -->\n    <div class=\"catattr-tab\">\n      <a-table\n        :columns=\"columns\"\n        :dataSource=\"data\"\n        :loading=\"loading\"\n        :pagination=\"pagination\"\n        :rowKey=\"record => record.id\"\n        @change=\"onChangePage\"\n        childrenColumnName=\"childrens\"\n      >\n        <template slot=\"name\" slot-scope=\"text, record\">\n          <a @click=\"onHandleInfo(record)\" href=\"javascript:;\">{{ text }}</a>\n        </template>\n      </a-table>\n    </div>\n    <!-- 抽屉 -->\n    <a-drawer\n      :destroyOnClose=\"true\"\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"closeDrawer\"\n      :visible=\"visible\"\n      :width=\"1040\"\n      :wrapStyle=\"{height: 'calc(100% - 108px)',overflow: 'auto',paddingBottom: '108px'}\"\n    >\n      <catattr-info @isDrawer=\"isDrawer\" :catattrInfo=\"catattrInfo\"/>\n    </a-drawer>\n  </div>\n</template>\n<script>\nimport CatattrInfo from './child_info/CatattrInfo'\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin'\nimport { columns } from './basic_lib/column'\nimport { getCategoryTree, getCatattrDetail } from './basic_lib/index'\nimport { setTimeout } from 'timers'\n\nexport default {\n  name: 'Catattr',\n  mixins: [mixinBasic, mixinDrawer],\n  data() {\n    return {\n      data: [],\n      columns,\n      pagination: {\n        total: 0,\n        defaultPageSize: 15,\n        defaultCurrent: 1,\n        showQuickJumper: true,\n        hideOnSinglePage: true,\n        showSizeChanger: true,\n        pageSizeOptions: ['10', '15', '30', '50', '100']\n      },\n      catattrInfo: {}\n    }\n  },\n  created() {\n    // 初始化Tab数据\n    getCategoryTree.call(this)\n  },\n  methods: {\n    // 查看详情\n    onHandleInfo(record) {\n      getCatattrDetail.call(this, record.id).then(() => {\n        this.visible = true\n      })\n      this.$store.commit('setPromptTitle', {\n        title: '产品属性详情',\n        isEdit: true\n      })\n    },\n    // 分页管理\n    onChangePage(pagination, filters, sorter) {\n      console.log(pagination.current)\n      getCategoryTree.call(this, pagination.current, pagination.pageSize)\n    },\n    // $emit改变isvisible\n    isDrawer(val) {\n      this.visible = val\n      // 关闭抽屉主动初始化\n      this.timer = setTimeout(() => {\n        getCategoryTree.call(this)\n      }, 100)\n    },\n    // 新建\n    showDrawer() {\n      this.visible = true\n      // this.catattrInfo = {}\n      this.$store.commit('setPromptTitle', {\n        title: '新建产品属性',\n        isEdit: false\n      })\n    }\n  },\n  components: {\n    'catattr-info': CatattrInfo\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.catattr {\n  .catattr-setup {\n    margin-bottom: 15px;\n  }\n  .catattr-remove {\n    margin: 0 10px;\n  }\n  .catattr-select-reset {\n    margin: 0 10px;\n  }\n  .catattr-select {\n    margin: 16px 0;\n  }\n}\n</style>\n"]}]}