/mixin/mixin'\nimport { statusChange } from '@/views/sale/mixin/minxinSale'\nimport { postJson } from '@/api/axios'\nimport api from '@/api/index'\nexport default {\n  mixins: [mixinBasic, mixinIsEdit, statusChange],\n  props: ['purchaseId', 'statusAction'],\n  components: {\n    GoodsInfo\n  },\n  data() {\n    return {\n      prchaseDetial: {},\n      edit: false,\n      visible: false\n    }\n  },\n  created() {\n    this.getProcurementDetial(this.purchaseId)\n  },\n  methods: {\n    isDisabled(val) {\n      this.disabled = !val\n    },\n    // 删除\n    isDelete(val) {\n      console.log(val)\n    },\n    // 获取渠道商采购详情\n    getProcurementDetial(id) {\n      postJson(api.getFactoryPrchaseDetial, { id }).then(res => {\n        if (res.data.code === 0) {\n          this.prchaseDetial = res.data.data\n        } else {\n          this.$message.error(res.data.msg)\n        }\n      })\n    },\n    // 接单/拒单\n    orderPick(type) {\n      const data = {}\n      data.id = this.purchaseId\n      data.status = type === 'lose' ? '5' : '1'\n      postJson(api.updateFactoryStatus, data).then(res => {\n        if (res.data.code === 0) {\n          this.$message.success('操作成功')\n          this.onClose()\n        }\n      })\n    },\n    // 编辑\n    orderEdit() {\n      this.edit = !this.edit\n    },\n    choseBom() {\n      this.visible = true\n    },\n    closeDrawer() {\n      this.visible = false\n    },\n    isDrawer(val) {\n      this.visible = val\n    }\n  }\n}\n",{"version":3,"sources":["purchaseInfo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuKA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"purchaseInfo.vue","sourceRoot":"src/views/factorySale/purchase/child_info","sourcesContent":["<template>\n  <div class=\"purchase-info\">\n    <div class=\"prompt-title\">\n      <a-row>\n        <a-col :span=\"18\">\n          <h3 class=\"prompt-h3\">工单详情</h3>\n        </a-col>\n        <a-col :span=\"6\" class=\"prompt-wrapper\" v-if=\"statusAction\">\n          <span class=\"prompt-edit\" @click=\"orderPick('pick')\">\n            <a-icon type=\"check-circle\" style=\"color:#F0BB2E;margin-right:4px\"/>\n            <span>接单</span>\n          </span>\n          <span class=\"prompt-delete\" @click=\"orderPick('lose')\">\n            <a-icon type=\"close-circle\" style=\"color:#D50304;margin-right:4px\"/>\n            <span>拒单</span>\n          </span>\n          <span class=\"prompt-delete\" @click=\"orderEdit\">\n            <a-icon style=\"color: green;margin-right: 4px;\" type=\"edit\"/>\n            <span>{{edit?'取消':'编辑'}}</span>\n          </span>\n        </a-col>\n      </a-row>\n    </div>\n    <a-row>\n      <a-col :span=\"24\">\n        <h3 class=\"purchase-info-title\">基本信息</h3>\n      </a-col>\n    </a-row>\n    <a-row>\n      <a-col :span=\"8\">\n        <a-form-item label=\"订单编码\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.odOrderNumber}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"销售单号\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>\n            <span>{{prchaseDetial.odOrderNumberO}}</span>\n          </template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"交期\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.odRequireDueDate}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"总价\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.odTotalPrice}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"工艺确认\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>\n            <a-switch :checked=\"prchaseDetial.craft\" disabled/>\n          </template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"收货方式\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.receiveWay}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"订单状态\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.status | stateFilter}}</template>\n        </a-form-item>\n      </a-col>\n    </a-row>\n    <a-row>\n      <a-col :span=\"24\">\n        <h3 class=\"purchase-info-title\">产品信息</h3>\n      </a-col>\n    </a-row>\n    <a-row>\n      <a-col :span=\"8\">\n        <a-form-item label=\"客户编码\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.custCode}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"产品名称\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.odSkuName}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"数量\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.odNumber}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"客户名称\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.custName}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"产品规格\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.odSpec}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"单价\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>{{prchaseDetial.odUnitPrice}}</template>\n        </a-form-item>\n      </a-col>\n      <a-col :span=\"8\">\n        <a-form-item label=\"当前版本\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n          <template>\n            <a href=\"javascript:;\">{{prchaseDetial.groupName}}</a>\n            <a href=\"javascript:;\" v-if=\"edit && !prchaseDetial.groupName\" @click=\"choseBom\">选择版本</a>\n          </template>\n        </a-form-item>\n      </a-col>\n    </a-row>\n    <a-row>\n      <a-col :span=\"24\">\n        <h3 class=\"purchase-info-title\">订单进度</h3>\n      </a-col>\n    </a-row>\n    <a-row>\n      <a-col :span=\"12\" style=\"margin-top:20px\">\n        <a-steps progressDot :current=\"prchaseDetial.num\" v-if=\"prchaseDetial.orderProgress\">\n          <template v-for=\"(item,index) in prchaseDetial.orderProgress.list\">\n            <a-step\n              :title=\"item.state | stateFilter\"\n              :key=\"index\"\n              :status=\"item.state === 'REJECTED'?'error':''\"\n            >\n              <template slot=\"description\">\n                <span class=\"stepC\">{{item.name}}</span>\n                <p class=\"stepC\">{{item.date}}</p>\n              </template>\n            </a-step>\n          </template>\n        </a-steps>\n      </a-col>\n    </a-row>\n    <div\n      :style=\"{\n          position: 'absolute',\n          left: 0,\n          bottom: 0,\n          width: '100%',\n          borderTop: '1px solid #E9E9E9',\n          padding: '10px 16px',\n          background: '#FFF',\n          textAlign: 'right',\n        }\"\n    >\n      <a-button :style=\"{marginRight: '8px'}\" @click=\"onClose\">取消</a-button>\n      <a-button type=\"primary\" @click=\"onClose\" :disabled=\"!edit\">保存</a-button>\n    </div>\n\n    <!-- 产品信息抽屉 -->\n    <a-drawer\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"closeDrawer\"\n      :visible=\"visible\"\n      :width=\"1080\"\n    >\n      <GoodsInfo @isDrawer=\"isDrawer\"></GoodsInfo>\n    </a-drawer>\n  </div>\n</template>\n\n<script>\nimport GoodsInfo from '@/views/product/pdm/goods/child_info/GoodsInfo'\nimport { mixinBasic, mixinIsEdit } from '@/components/tools/mixin/mixin'\nimport { statusChange } from '@/views/sale/mixin/minxinSale'\nimport { postJson } from '@/api/axios'\nimport api from '@/api/index'\nexport default {\n  mixins: [mixinBasic, mixinIsEdit, statusChange],\n  props: ['purchaseId', 'statusAction'],\n  components: {\n    GoodsInfo\n  },\n  data() {\n    return {\n      prchaseDetial: {},\n      edit: false,\n      visible: false\n    }\n  },\n  created() {\n    this.getProcurementDetial(this.purchaseId)\n  },\n  methods: {\n    isDisabled(val) {\n      this.disabled = !val\n    },\n    // 删除\n    isDelete(val) {\n      console.log(val)\n    },\n    // 获取渠道商采购详情\n    getProcurementDetial(id) {\n      postJson(api.getFactoryPrchaseDetial, { id }).then(res => {\n        if (res.data.code === 0) {\n          this.prchaseDetial = res.data.data\n        } else {\n          this.$message.error(res.data.msg)\n        }\n      })\n    },\n    // 接单/拒单\n    orderPick(type) {\n      const data = {}\n      data.id = this.purchaseId\n      data.status = type === 'lose' ? '5' : '1'\n      postJson(api.updateFactoryStatus, data).then(res => {\n        if (res.data.code === 0) {\n          this.$message.success('操作成功')\n          this.onClose()\n        }\n      })\n    },\n    // 编辑\n    orderEdit() {\n      this.edit = !this.edit\n    },\n    choseBom() {\n      this.visible = true\n    },\n    closeDrawer() {\n      this.visible = false\n    },\n    isDrawer(val) {\n      this.visible = val\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/variable.less\";\n.inputWidth {\n  .inputWidth();\n}\n.purchase-info {\n  .purchase-info-title {\n    .h3_title();\n  }\n  .labelColor {\n    /deep/ .ant-form-item-label label {\n      .labelColor();\n    }\n  }\n}\n\n.prompt-title {\n  border-bottom: 1px solid #ebedf0;\n  min-height: 40px;\n  margin-bottom: 10px;\n  .prompt-wrapper {\n    display: flex;\n    justify-content: flex-end;\n    .prompt-edit {\n      margin: 0 10px;\n      cursor: pointer;\n    }\n    .prompt-delete {\n      cursor: pointer;\n      margin-right: 10px;\n    }\n    .prompt-close {\n      cursor: pointer;\n      padding: 0 10px;\n      border-left: 1px solid #d8d8d8;\n    }\n  }\n}\n/deep/ .ant-steps-item-finish > .ant-steps-item-content > .ant-steps-item-title,\n/deep/ .ant-steps-item-wait > .ant-steps-item-content > .ant-steps-item-title {\n  font-size: 14px;\n  color: #666;\n}\n/deep/\n  .ant-steps-item-process\n  > .ant-steps-item-content\n  > .ant-steps-item-title {\n  font-size: 14px;\n  color: #333333;\n}\n.stepC {\n  color: #666;\n  font-size: 12px;\n}\n</style>\n"]}]}