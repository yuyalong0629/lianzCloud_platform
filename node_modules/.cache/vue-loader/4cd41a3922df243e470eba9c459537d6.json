 <a-select\n                  style=\"width:160px\"\n                  showSearch\n                  :allowClear=\"true\"\n                  placeholder=\"请选择规格\"\n                  @change=\"(val)=>{inputChange(val,'spec')}\"\n                  @popupScroll=\"e=>currentPageChange(e,'spec')\"\n                  :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n                >\n                  <a-select-option v-for=\"(item,index) in odSpecList\" :key=\"index\">{{item}}</a-select-option>\n                </a-select>\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"8\">\n              <a-form-item label=\"交期\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n                <a-range-picker\n                  style=\"width:160px\"\n                  format=\"YYYY-MM-DD HH:mm:ss\"\n                  @change=\"(dates, dateStrings)=>{inputChange(dateStrings,'date')}\"\n                />\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"8\">\n              <a-form-item label=\"材质\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n                <a-select\n                  style=\"width:160px\"\n                  showSearch\n                  :allowClear=\"true\"\n                  placeholder=\"请选择材质\"\n                  @change=\"(val)=>{inputChange(val,'cz')}\"\n                  @popupScroll=\"e=>currentPageChange(e,'cz')\"\n                  :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n                >\n                  <a-select-option v-for=\"(item,index) in textures\" :key=\"index\">{{item}}</a-select-option>\n                </a-select>\n              </a-form-item>\n            </a-col>\n          </a-row>\n        </div>\n        <div class=\"order-list\">\n          <div\n            class=\"order-list-item demo-infinite-container\"\n            v-infinite-scroll=\"handleInfiniteOnLoad\"\n            :infinite-scroll-disabled=\"busy\"\n            :infinite-scroll-distance=\"10\"\n          >\n            <a-list :dataSource=\"dataCompany\">\n              <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n                <div class=\"list-item\">\n                  <div class=\"item-left\">\n                    <div class=\"item-left-top\">\n                      <a href=\"javascript:;\" @click=\"infoView(item.id)\">{{item.name}}__{{item.bom}}</a>\n                      <span>{{item.craft}}</span>\n                    </div>\n                    <ul class=\"item-left-desc\">\n                      <li>{{item.spec}}</li>\n                      <li>{{item.cz}}</li>\n                      <li>{{item.bmcl}}</li>\n                      <li>{{moment(item.odPromiseDueDate).format(dateFormat)}}</li>\n                      <li>{{item.odNumber}}</li>\n                    </ul>\n                  </div>\n                  <div class=\"item-right\">\n                    <a-popover trigger=\"click\" :visible=\"index ===updateList.index\">\n                      <template slot=\"content\">\n                        <a-form-item\n                          label=\"交期\"\n                          :label-col=\"{ span: 5,offset:0 }\"\n                          :wrapper-col=\"{span:12}\"\n                          class=\"mb\"\n                        >\n                          <a-date-picker\n                            showTime\n                            format=\"YYYY-MM-DD HH:mm:ss\"\n                            :defaultValue=\"moment(updateList.date, 'YYYY-MM-DD HH:mm:ss')\"\n                            style=\"width:170px;\"\n                            @change=\"updateTime\"\n                          />\n                        </a-form-item>\n                        <a-form-item\n                          label=\"数量\"\n                          :label-col=\"{ span: 5,offset:0 }\"\n                          :wrapper-col=\"{span:12}\"\n                          class=\"mb\"\n                        >\n                          <a-input-number\n                            :min=\"1\"\n                            :max=\"item.odNumber\"\n                            v-model=\"updateList.num\"\n                            style=\"width:170px;\"\n                          />\n                        </a-form-item>\n                        <a-form-item class=\"mb\">\n                          <a-button type=\"danger\" @click=\"removeItem(item.id)\">移除</a-button>\n                        </a-form-item>\n                        <a-form-item class=\"mb\">\n                          <a-button type=\"primary\" @click=\"addCompany(item.id,index,'addLittle')\">确定</a-button>\n                        </a-form-item>\n                      </template>\n                      <a-icon\n                        type=\"ellipsis\"\n                        class=\"item-right-edit\"\n                        @click=\"updataDes(item.odNumber,item.odPromiseDueDate,index)\"\n                      />\n                    </a-popover>\n                    <a-button\n                      type=\"primary\"\n                      shape=\"circle\"\n                      icon=\"plus\"\n                      @click=\"addCompany(item.id,index,'addAll')\"\n                    ></a-button>\n                  </div>\n                </div>\n              </a-list-item>\n              <div v-if=\"loadingCompony && !busy\" class=\"demo-loading-container\">\n                <a-spin/>\n              </div>\n            </a-list>\n          </div>\n        </div>\n      </a-col>\n      <a-col :span=\"12\" class=\"dispatch-right\">\n        <div class=\"title\">\n          <span @click=\"showDrawer\">{{companyName}}</span>\n          <span @click=\"showDrawer\" v-if=\"companyVisible\">\n            <a-icon type=\"double-right\"/>\n          </span>\n          <span @click=\"showDrawer\">\n            <a-icon type=\"double-left\"/>\n          </span>\n        </div>\n        <div class=\"order-list demo-infinite-container\">\n          <a-list :dataSource=\"companyList\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n              <div class=\"list-item\">\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a href=\"javascript:;\">{{item.name}}__{{item.bom}}</a>\n                    <span>{{item.craft}}</span>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.spec}}</li>\n                    <li>{{item.cz}}</li>\n                    <li>{{item.bmcl}}</li>\n                    <li>{{moment(item.odPromiseDueDate).format(dateFormat)}}</li>\n                    <li>{{item.odNumber}}</li>\n                  </ul>\n                </div>\n                <div class=\"item-right\">\n                  <a-button\n                    type=\"danger\"\n                    shape=\"circle\"\n                    icon=\"minus\"\n                    style=\"background:#f5222d;color:#fff;\"\n                    @click=\"minusCompany(item.id,index)\"\n                  ></a-button>\n                </div>\n              </div>\n            </a-list-item>\n          </a-list>\n          <div style=\"height:40px\"></div>\n          <div class=\"dispatch-right-bottom\">\n            <a-button>取消</a-button>\n            <a-button\n              type=\"primary\"\n              :disabled=\"!companyList.length\"\n              @click=\"submit\"\n              :loading=\"submitLoading\"\n            >确定</a-button>\n          </div>\n        </div>\n      </a-col>\n    </a-row>\n    <!-- 工厂名称抽屉 -->\n    <a-drawer\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"onClose\"\n      :visible=\"companyVisible\"\n      :width=\"300\"\n    >\n      <companyName :factoryId=\"factoryId\" @selectCompany=\"selectName\"></companyName>\n    </a-drawer>\n\n    <!-- 产品信息抽屉 -->\n    <a-drawer\n      placement=\"right\"\n      :closable=\"false\"\n      @close=\"closeDrawer\"\n      :visible=\"visible\"\n      :width=\"1080\"\n    >\n      <dispatchInfo @isDrawer=\"isDrawer\"></dispatchInfo>\n    </a-drawer>\n  </div>\n</template>\n\n<script>\nimport infiniteScroll from 'vue-infinite-scroll'\nimport { postJson, get } from '@/api/axios.js'\nimport api from '@/api/index.js'\nimport moment from 'moment'\nimport companyName from './child_lib/companyName'\nimport dispatchInfo from './child_lib/dispatchInfo'\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin'\nimport { mixinCurrentPage, mixinGetProductAbout } from '@/views/sale/mixin/minxinSale'\nexport default {\n  directives: { infiniteScroll },\n  mixins: [mixinBasic, mixinDrawer, mixinCurrentPage, mixinGetProductAbout],\n  components: {\n    companyName,\n    dispatchInfo\n  },\n  data() {\n    return {\n      factoryId: '',\n      dateFormat: 'YYYY-MM-DD',\n      busy: false,\n      companyVisible: false,\n      loadingCompony: false,\n      dataCompanySize: 0,\n      dataCompany: [],\n      factoryOrderDetailPostBean: {\n        currentPage: 1,\n        pageSize: 15,\n        cz: '',\n        spec: '',\n        startTime: '',\n        endTime: ''\n      },\n      companyName: '请选择工厂',\n      companyList: [],\n      companyNameList: [],\n      updateList: {\n        index: -1,\n        num: 0,\n        date: ''\n      },\n      goodsInfo: {\n        currentPage: 1,\n        pageSize: 15\n      },\n      submitLoading: false,\n      timer: ''\n    }\n  },\n  created() {\n    this.getListCompanySpringCz(this.specCurrentPage)\n    this.getListCompanySpringSpecs(this.czCurrentPage)\n  },\n  methods: {\n    moment,\n    // 按条件搜索渠道商订单列表\n    inputChange(val, type) {\n      const factoryOrderDetailPostBean = this.factoryOrderDetailPostBean\n      factoryOrderDetailPostBean.currentPage = 1\n      switch (type) {\n        case 'spec':\n          factoryOrderDetailPostBean.spec = this.specification[val]\n          break\n        case 'cz':\n          factoryOrderDetailPostBean.cz = this.textures[val]\n          break\n        case 'date':\n          factoryOrderDetailPostBean.startTime = val[0]\n          factoryOrderDetailPostBean.endTime = val[1]\n          break\n      }\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.getFactoryOrder()\n      }, 1000)\n    },\n    // 滚动滑动条加载订单\n    handleInfiniteOnLoad() {\n      const data = this.dataCompany\n      this.loadingCompony = true\n      if (data.length >= this.dataCompanySize && this.dataCompanySize !== 0) {\n        this.$message.warning('暂时没有更多订单数据')\n        this.busy = true\n        this.loadingCompony = false\n      } else {\n        this.getFactoryOrder()\n        this.loadingCompony = false\n      }\n    },\n    // 移除订单\n    removeItem(id) {\n      var _this = this\n      this.$confirm({\n        title: '移除订单信息',\n        content: '您确定要删除这条订单消息吗?',\n        zIndex: 99999,\n        onOk() {\n          postJson(api.delFactoryOrder, { id: id }).then(res => {\n            if (res.data.code === 0) {\n              _this.updateList.index = -1\n              _this.$message.success('移除成功', 1)\n              _this.factoryOrderDetailPostBean.currentPage = 1\n              _this.getFactoryOrder()\n            }\n          })\n        },\n        onCancel() { }\n      })\n    },\n    // 打开抽屉\n    showDrawer() {\n      this.companyVisible = !this.companyVisible\n    },\n    // 关闭抽屉\n    onClose() {\n      this.companyVisible = false\n    },\n    // 分派订单到工厂\n    addCompany(id, index, type) {\n      const dataCompany = this.dataCompany\n      const companyList = this.companyList\n      const updateList = this.updateList\n      updateList.index = -1\n      const tar = companyList.filter(item => item.id === id)[0]\n      if (tar) {\n        if (type === 'addAll') {\n          tar.odNumber += dataCompany[index].odNumber\n          dataCompany.splice(index, 1)\n        } else {\n          if (updateList.num >= dataCompany[index].odNumber) {\n            dataCompany.splice(index, 1)\n          } else {\n            dataCompany[index].odNumber -= updateList.num\n          }\n          tar.odPromiseDueDate = updateList.date\n          tar.odNumber += updateList.num\n        }\n      } else {\n        if (type === 'addAll') {\n          companyList.push(dataCompany[index])\n          dataCompany.splice(index, 1)\n        } else {\n          const target = dataCompany[index]\n          if (updateList.num >= target.odNumber) {\n            target.odPromiseDueDate = updateList.date\n            companyList.push(target)\n            dataCompany.splice(index, 1)\n          } else {\n            const team = JSON.parse(JSON.stringify(target))\n            target.odNumber -= updateList.num\n            team.odNumber = updateList.num\n            team.odPromiseDueDate = updateList.date\n            companyList.push(team)\n          }\n        }\n      }\n    },\n    // 删除工厂中的订单\n    minusCompany(id, index) {\n      const dataCompany = this.dataCompany\n      const companyList = this.companyList\n      const target = dataCompany.filter(item => item.id === id)[0]\n      if (target) {\n        target.odNumber += companyList[index].odNumber\n      } else {\n        dataCompany.push(companyList[index])\n      }\n      companyList.splice(index, 1)\n    },\n    // 获取渠道商订单列表\n    getFactoryOrder() {\n      postJson(api.getFactoryOrder, this.factoryOrderDetailPostBean).then(res => {\n        if (res.data.code === 0) {\n          this.dataCompanySize = Number(res.data.data.total)\n          if (this.dataCompany.length) {\n            this.dataCompany = this.dataCompany.concat(res.data.data.records)\n          } else {\n            this.dataCompany = res.data.data.records\n          }\n        }\n        if (this.factoryOrderDetailPostBean.currentPage < Number(res.data.data.pages)) {\n          this.factoryOrderDetailPostBean.currentPage += 1\n        }\n      })\n    },\n    // 设置选中的订单产品的数量和交期\n    updataDes(num, data, index) {\n      const updateList = this.updateList\n      if (updateList.index === -1) {\n        updateList.index = index\n        updateList.num = num\n        updateList.date = data\n      } else {\n        updateList.index = -1\n      }\n    },\n    updateTime(dates, dateStrings) {\n      this.updateList.date = dateStrings\n    },\n    // 订单分派提交\n    submit() {\n      if (this.factoryId === '') {\n        this.$message.warning('请选择工厂')\n        return\n      }\n      const addFactorySaleOrderPostBean = {\n        factoryId: this.factoryId,\n        list: []\n      }\n      const list = addFactorySaleOrderPostBean.list\n      const companyList = this.companyList\n      companyList.forEach(item => {\n        list.push(\n          {\n            id: item.id,\n            number: item.odNumber,\n            odRequireDueDate: item.odPromiseDueDate\n          }\n        )\n      })\n      this.submitLoading = true\n      postJson(api.addFactoryOrder, addFactorySaleOrderPostBean).then(res => {\n        if (res.data.code === 0) {\n          this.$message.success('分派成功')\n          this.submitLoading = false\n          this.factoryOrderDetailPostBean.currentPage = 1\n          this.companyList = []\n          this.dataCompany = []\n          this.getFactoryOrder()\n        }\n      })\n    },\n    // 选择框的下拉加载\n    currentPageChange(e, type) {\n      const result = this.getCurrentPageChange(e)\n      if (result) {\n        if (type === 'cz') {\n          this.getListCompanySpringCz(this.specCurrentPage)\n        } else if (type === 'spec') {\n          this.getListCompanySpringSpecs(this.czCurrentPage)\n        }\n      }\n    },\n    // 替换显示工厂名字以及工厂id\n    selectName(data) {\n      this.factoryId = data.id\n      this.companyName = data.name\n      this.companyVisible = false\n    },\n    // 关闭抽屉\n    isDrawer(val) {\n      this.visible = val\n    },\n    // 查看产品详情\n    infoView(id) {\n      this.visible = true\n      // this.purchaseId = id\n      this.$store.commit('setPromptTitle', {\n        title: '产品信息',\n        isEdit: true\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/variable.less\";\n.inputWidth {\n  .inputWidth();\n}\n.dispatch {\n  .search,\n  .title {\n    height: 63px;\n  }\n  .dispatch-left,\n  .dispatch-right {\n    padding: 10px;\n  }\n  .dispatch-left {\n    border-right: 1px solid #ddd;\n  }\n}\n.order-list {\n  width: 100%;\n  .order-list-item {\n    position: relative;\n  }\n  .list-item {\n    width: 100%;\n    height: 80px;\n    padding: 15px 5px;\n    border-bottom: 1px solid #eee;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    .item-left-top {\n      text-align: left;\n      & > a {\n        font-size: 16px;\n        margin-right: 10px;\n      }\n      & > span {\n        font-size: 16px;\n        color: #f78016;\n      }\n    }\n    .item-left-desc {\n      li {\n        font-size: 14px;\n        color: #666666;\n        float: left;\n        padding: 5px 0;\n        &::after {\n          content: \"\";\n          padding: 5px 0;\n          display: inline-block;\n          border-right: 1px solid #d8d8d8;\n          margin: 0 10px;\n        }\n      }\n    }\n    .item-right {\n      line-height: 50px;\n      button {\n        width: 25px;\n        height: 25px;\n      }\n    }\n    .item-right-edit {\n      width: 32px;\n      height: 32px;\n      cursor: pointer;\n      display: inline-block;\n      color: #666;\n      margin-right: 20px;\n      font-size: 20px;\n    }\n  }\n}\n.ant-popover-inner .mb {\n  margin-bottom: 5px;\n  text-align: center;\n  button {\n    width: 100%;\n  }\n}\n.dispatch-right {\n  position: relative;\n  padding: 10px 0 !important;\n  .title span:first-child {\n    cursor: pointer;\n    display: inline-block;\n    padding: 0 20px;\n    line-height: 50px;\n    font-size: 16px;\n    color: #5077d1;\n    background-color: #f8faff;\n    border-radius: 0px 6px 6px 0px;\n  }\n  .title span:nth-child(2) {\n    cursor: pointer;\n    display: inline-block;\n    position: absolute;\n    right: 280px;\n    color: #fff;\n    line-height: 40px;\n    font-size: 24px;\n    width: 40px;\n    text-align: center;\n    border-radius: 50%;\n    background-image: linear-gradient(90deg, #6487fe 0%, #c971f2 100%);\n    z-index: 9999;\n  }\n  .title span:last-child {\n    display: inline-block;\n    cursor: pointer;\n    position: absolute;\n    right: 0;\n    color: #fff;\n    line-height: 40px;\n    font-size: 24px;\n    width: 40px;\n    text-align: center;\n    border-radius: 50%;\n    z-index: 0;\n    background-image: linear-gradient(90deg, #6487fe 0%, #c971f2 100%);\n  }\n  .order-list {\n    padding-left: 10px;\n  }\n  .dispatch-right-bottom {\n    position: absolute;\n    width: 100%;\n    height: 50px;\n    line-height: 50px;\n    background-color: #fff;\n    // position: fixed;\n    bottom: 0px;\n    text-align: center;\n    & > button {\n      margin: 0 20px;\n    }\n  }\n}\n</style>\n<style>\n.ant-popover-inner .ant-popover-buttons {\n  display: none !important;\n}\n.ant-popover {\n  z-index: 999;\n}\n.ant-list-item {\n  padding: 0 !important;\n}\n.demo-infinite-container {\n  border-radius: 4px;\n  overflow: auto;\n  padding: 8px 24px;\n  min-height: 80vh;\n  max-height: 84vh;\n}\n.demo-loading-container {\n  position: absolute;\n  bottom: 40px;\n  width: 100%;\n  text-align: center;\n}\n</style>\n"]}]}