n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n@import \"~styles/dispach.less\";\n",{"version":3,"sources":["purchasList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwXA","file":"purchasList.vue","sourceRoot":"src/views/procurement/purchasList","sourcesContent":["<template>\n  <div class=\"dispatch\">\n    <a-row>\n      <a-col :span=\"12\" class=\"dispatch-left\">\n        <a-row class=\"search\">\n          <a-col :span=\"8\">\n            <a-form-item label=\"规格\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-select\n                style=\"width:160px\"\n                showSearch\n                :allowClear=\"true\"\n                placeholder=\"请选择规格\"\n                @change=\"(val)=>{inputChange(val,'spec')}\"\n                @popupScroll=\"e=>currentPageChange(e,'spec')\"\n                :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n              >\n                <a-select-option v-for=\"item in odSpecList\" :key=\"item\">{{item}}</a-select-option>\n              </a-select>\n            </a-form-item>\n          </a-col>\n          <a-col :span=\"8\">\n            <a-form-item label=\"材质\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\n              <a-select\n                style=\"width:160px\"\n                showSearch\n                :allowClear=\"true\"\n                placeholder=\"请选择材质\"\n                @change=\"(val)=>{inputChange(val,'cz')}\"\n                @popupScroll=\"e=>currentPageChange(e,'cz')\"\n                :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n              >\n                <a-select-option v-for=\"item in textures\" :key=\"item\">{{item}}</a-select-option>\n              </a-select>\n            </a-form-item>\n          </a-col>\n        </a-row>\n        <div class=\"order-list\">\n          <div\n            class=\"order-list-item demo-infinite-container\"\n            v-infinite-scroll=\"handleInfiniteOnLoad\"\n            :infinite-scroll-disabled=\"busy\"\n            :infinite-scroll-distance=\"10\"\n          >\n            <a-list :dataSource=\"dataCompany\">\n              <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n                <div\n                  class=\"list-item\"\n                  :class=\"index === setIndex? 'active':''\"\n                  @click=\"addCompany(item.id,index)\"\n                >\n                  <div class=\"item-left\">\n                    <div class=\"item-left-top\">\n                      <a href=\"javascript:;\">{{item.sku.name}}</a>\n                      <span>{{item.sku.spec}}</span>\n                    </div>\n                    <ul class=\"item-left-desc\">\n                      <li>{{item.sku.cz}}</li>\n                      <li>{{item.sku.standard}}</li>\n                      <li>{{item.sku.unit}}</li>\n                      <li>当前库存：{{item.skuPriceInventory?item.skuPriceInventory.storeCount:0}}</li>\n                    </ul>\n                  </div>\n                  <div class=\"item-right\">\n                    <a-popover trigger=\"click\" v-model=\"item.clickedVisible\">\n                      <template slot=\"content\">\n                        <a-form-item\n                          label=\"紧急程度:\"\n                          :label-col=\"{ span: 12}\"\n                          :wrapper-col=\"{span:12}\"\n                        >\n                          <a-switch/>\n                        </a-form-item>\n                        <a-form-item class=\"mb\">\n                          <a-button type=\"danger\" @click=\"removeItem(item.id,index)\">移除</a-button>\n                        </a-form-item>\n                      </template>\n                      <a-icon\n                        type=\"ellipsis\"\n                        class=\"item-right-edit\"\n                        @click=\"e=> updataDes(e,index)\"\n                      />\n                    </a-popover>\n                    <a-button type=\"primary\" shape=\"circle\" icon=\"plus\"></a-button>\n                  </div>\n                </div>\n              </a-list-item>\n              <div v-if=\"loadingCompony && !busy\" class=\"demo-loading-container\">\n                <a-spin/>\n              </div>\n            </a-list>\n            <div class=\"list-item\">\n              <div class=\"item-left\"></div>\n              <div class=\"item-right-add\">\n                <a href=\"javascript:;\" @click=\"addMaterial\">添加新材料</a>\n                <span>\n                  点击这里\n                  <a href=\"javascript:;\" @click=\"flushPurchaseOrder\">刷新</a> 刷新供应商\n                </span>\n              </div>\n            </div>\n          </div>\n        </div>\n      </a-col>\n      <a-col :span=\"12\" class=\"dispatch-right procurement\">\n        <div class=\"title\"></div>\n        <div class=\"order-list demo-infinite-container\">\n          <template v-for=\"(item,index) in companyList\">\n            <div class=\"list-item\" :key=\"index\">\n              <div class=\"item-left\">\n                <div class=\"item-left-top\">\n                  <a href=\"javascript:;\">{{item.company.companyName}}</a>\n                </div>\n                <ul class=\"item-left-desc\">\n                  <li>库存：{{item.sku.storeCount}}</li>\n                  <li>{{item.sku.unit}}</li>\n                  <li>{{item.sku.price}}</li>\n                </ul>\n              </div>\n              <div class=\"item-right\">\n                <a-input-number v-model=\"item.number\"/>\n              </div>\n            </div>\n          </template>\n          <div class=\"list-item\" v-show=\"setIndex !== ''\">\n            <div class=\"item-left\"></div>\n            <div class=\"item-right\">\n              <div class=\"item-right-add\">\n                <a href=\"javascript:;\" @click=\"addSupplier\">添加新供应商</a>\n                <span>\n                  点击这里\n                  <a href=\"javascript:;\" @click=\"flushMaterialOrder\">刷新</a> 刷新供应商\n                </span>\n              </div>\n            </div>\n          </div>\n          <div style=\"height:40px\"></div>\n          <div class=\"dispatch-right-bottom\">\n            <a-button @click=\"handleClose\">取消</a-button>\n            <a-button type=\"primary\" @click=\"submit\" :loading=\"submitLoading\">确定</a-button>\n          </div>\n        </div>\n      </a-col>\n    </a-row>\n    <a-modal v-model=\"visible\" title=\"添加供应商\" width=\"820px\" :footer=\"false\">\n      <searchSupplier v-if=\"visible\" :supplier=\"supplier\"></searchSupplier>\n    </a-modal>\n    <a-modal v-model=\"meterial\" title=\"添加材料\" width=\"820px\" :footer=\"false\">\n      <newMaterial v-if=\"meterial\"></newMaterial>\n    </a-modal>\n  </div>\n</template>\n\n<script>\nimport infiniteScroll from 'vue-infinite-scroll'\nimport searchSupplier from './other/searchSupplier'\nimport newMaterial from './other/newMaterial'\nimport { get, postJson, deletedJson } from '@/api/axios.js'\nimport api from '@/api/index.js'\nimport { mixinBasic, mixinDrawer } from '@/components/tools/mixin/mixin'\nimport { mixinCurrentPage, mixinGetProductAbout } from '@/views/sale/mixin/minxinSale'\nexport default {\n  directives: { infiniteScroll },\n  mixins: [mixinBasic, mixinDrawer, mixinCurrentPage, mixinGetProductAbout],\n  components: {\n    searchSupplier,\n    newMaterial\n  },\n  data() {\n    return {\n      busy: false,\n      loadingCompony: false,\n      dataCompanySize: 0,\n      dataCompany: [],\n      factoryOrderDetailPostBean: {\n        currentPage: 1,\n        pageSize: 15,\n        cz: '',\n        spec: '',\n        standard: ''\n      },\n      companyList: [],\n      submitLoading: false,\n      timer: '',\n      setIndex: '',\n      meterial: false,\n      supplier: {\n        id: '',\n        factoryMaterialId: '',\n        cz: '',\n        spec: '',\n        standard: ''\n      }\n    }\n  },\n  created() {\n    this.getFactoryMaterialRequireCz(this.specCurrentPage)\n    this.getFactoryMaterialRequireSpec(this.czCurrentPage)\n  },\n  methods: {\n    // 按条件搜索\n    inputChange(val, type) {\n      this.dataCompany = []\n      this.companyList = []\n      this.setIndex = ''\n      const factoryOrderDetailPostBean = this.factoryOrderDetailPostBean\n      factoryOrderDetailPostBean.currentPage = 1\n      factoryOrderDetailPostBean[type] = val\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.getFactoryOrder()\n      }, 1000)\n    },\n    // 滚动滑动条加载订单\n    handleInfiniteOnLoad() {\n      const data = this.dataCompany\n      this.loadingCompony = true\n      if (data.length >= this.dataCompanySize && this.dataCompanySize !== 0) {\n        this.$message.warning('暂时没有更多订单数据')\n        this.busy = true\n        this.loadingCompony = false\n      } else {\n        this.getFactoryOrder()\n        this.loadingCompony = false\n      }\n    },\n    // 移除采购订单\n    removeItem(id, index) {\n      var _this = this\n      this.$confirm({\n        title: '移除订单信息',\n        content: '您确定要删除这条采购订单消息吗?',\n        zIndex: 99999,\n        onOk() {\n          deletedJson(api.deleteMaterialOrder, [id]).then(res => {\n            if (res.data.code === 0) {\n              _this.$message.success('移除成功')\n              _this.setIndex = ''\n              _this.companyList = []\n              _this.dataCompany.splice(index, 1)\n            } else {\n              _this.$message.error(res.data.msg)\n            }\n          })\n        }\n      })\n    },\n    addCompany(id, index) {\n      this.setIndex = index\n      const data = this.dataCompany[index].skuSupplierList\n      if (data.length > 0) {\n        const _this = this\n        data.forEach(item => {\n          _this.$set(data, 'number', 0)\n        })\n        this.companyList = data\n      }\n    },\n    // 取消操作\n    handleClose() {\n      this.setIndex = ''\n      this.companyList = []\n    },\n    // 获取带采购列表\n    getFactoryOrder() {\n      get(api.getPurchasingManagement, this.factoryOrderDetailPostBean).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data.records\n          if (data.length > 0) {\n            data.forEach(item => {\n              item.clickedVisible = false\n            })\n          }\n          this.dataCompanySize = Number(res.data.data.total)\n          this.dataCompany = this.dataCompany.concat(data)\n        }\n        this.factoryOrderDetailPostBean.currentPage += 1\n      })\n    },\n    updataDes(e, index) {\n      window.event ? window.event.cancelBubble = true : e.stopPropagation()\n      this.dataCompany[index].clickedVisible = true\n    },\n    // 刷新采购单\n    flushPurchaseOrder() {\n      this.dataCompany = []\n      this.companyList = []\n      this.setIndex = ''\n      const factoryOrderDetailPostBean = this.factoryOrderDetailPostBean\n      factoryOrderDetailPostBean.currentPage = 1\n      this.getFactoryOrder()\n    },\n    // 刷新\n    flushMaterialOrder() {\n      const _this = this\n      const factoryId = this.dataCompany[this.setIndex].factoryId\n      const materialId = this.dataCompany[this.setIndex].materialId\n      get(api.flushMaterialOrder + `/${factoryId}/${materialId}`).then(res => {\n        if (res.data.code === 0) {\n          const data = res.data.data\n          data.forEach(item => {\n            _this.$set(data, 'number', 0)\n          })\n          this.companyList = data\n        }\n      })\n    },\n    // 提交供应商销售单确认\n    submit() {\n      if (this.companyList.length === 0) { return }\n      this.submitLoading = true\n      const data = {}\n      const supplierAssignmentList = []\n      const companyList = this.companyList\n      data.id = this.dataCompany[this.setIndex].id\n      companyList.forEach(item => {\n        if (item.number > 0) {\n          supplierAssignmentList.push({\n            number: item.number,\n            supplierId: item.supplierId,\n            supplierMaterialId: item.supplierMaterialId\n          })\n        }\n      })\n      data.supplierAssignmentList = [...supplierAssignmentList]\n      if (data.supplierAssignmentList.length === 0) {\n        this.submitLoading = false\n        return\n      }\n      postJson(api.addMaterialOrderSale, data).then(res => {\n        if (res.data.code === 0) {\n          this.$message.success('新增供应商采购成功')\n          this.companyList = []\n          this.dataCompany = []\n          this.setIndex = ''\n          this.factoryOrderDetailPostBean.currentPage = 1\n          this.getFactoryOrder()\n        } else {\n          this.$message.error(res.data.msg)\n        }\n        this.submitLoading = false\n      })\n    },\n    currentPageChange(e, type) {\n      const result = this.getCurrentPageChange(e)\n      if (result) {\n        if (type === 'cz') {\n          this.getFactoryMaterialRequireCz(this.czCurrentPage)\n        } else if (type === 'spec') {\n          this.getFactoryMaterialRequireSpec(this.specCurrentPage)\n        }\n      }\n    },\n    isDrawer(val) {\n      this.visible = val\n    },\n    addSupplier() {\n      const data = this.dataCompany[this.setIndex]\n      this.supplier = {\n        id: data.id,\n        factoryMaterialId: data.materialId,\n        cz: data.sku.cz,\n        spec: data.sku.spec,\n        standard: data.sku.standard\n      }\n      this.visible = true\n    },\n    addMaterial() {\n      this.meterial = true\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/dispach.less\";\n</style>\n"]}]}