             <a-form-item\n                label=\"交期\"\n                :label-col=\"formItemLayout.labelCol\"\n                :wrapper-col=\"formItemLayout.wrapperCol\"\n              >\n                <a-range-picker\n                  class=\"inputWidth\"\n                  @change=\"(dates, dateStrings)=>{inputChange(dateStrings,'date')}\"\n                />\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"8\">\n              <a-form-item\n                label=\"材质\"\n                :label-col=\"formItemLayout.labelCol\"\n                :wrapper-col=\"formItemLayout.wrapperCol\"\n              >\n                <a-select\n                  class=\"inputWidth\"\n                  showSearch\n                  :allowClear=\"true\"\n                  placeholder=\"请选择材质\"\n                  @change=\"(val)=>{inputChange(val,'cz')}\"\n                  @popupScroll=\"e=>currentPageChange(e)\"\n                  :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n                >\n                  <a-select-option v-for=\"(item,index) in textures\" :key=\"index\">{{item}}</a-select-option>\n                </a-select>\n              </a-form-item>\n            </a-col>\n          </a-row>\n        </div>\n        <div class=\"order-list\">\n          <div\n            class=\"order-list-item demo-infinite-container\"\n            v-infinite-scroll=\"handleInfiniteOnLoad\"\n            :infinite-scroll-disabled=\"busy\"\n            :infinite-scroll-distance=\"10\"\n          >\n            <a-list :dataSource=\"dataCompany\">\n              <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n                <div class=\"list-item\">\n                  <div class=\"item-left\">\n                    <div class=\"item-left-top\">\n                      <a href=\"javascript:;\">{{item.name}}__{{item.bom}}</a>\n                      <span>{{item.craft}}</span>\n                    </div>\n                    <ul class=\"item-left-desc\">\n                      <li>{{item.spec}}</li>\n                      <li>{{item.cz}}</li>\n                      <li>{{item.bmcl}}</li>\n                      <li>{{moment(item.odPromiseDueDate).format(dateFormat)}}</li>\n                      <li>{{item.odNumber}}</li>\n                    </ul>\n                  </div>\n                  <div class=\"item-right\">\n                    <a-popover trigger=\"click\" :visible=\"index ===updateList.index\">\n                      <template slot=\"content\">\n                        <a-form-item\n                          label=\"交期\"\n                          :label-col=\"formItemLayout.labelCol\"\n                          :wrapper-col=\"formItemLayout.wrapperCol\"\n                          class=\"mb\"\n                        >\n                          <a-date-picker\n                            :value=\"moment(updateList.date, dateFormat)\"\n                            style=\"width:200px;\"\n                            @change=\"updateTime\"\n                          />\n                        </a-form-item>\n                        <a-form-item\n                          label=\"数量\"\n                          :label-col=\"formItemLayout.labelCol\"\n                          :wrapper-col=\"formItemLayout.wrapperCol\"\n                          class=\"mb\"\n                        >\n                          <a-input-number\n                            :min=\"1\"\n                            :max=\"item.odNumber\"\n                            v-model=\"updateList.num\"\n                            style=\"width:200px;\"\n                          />\n                        </a-form-item>\n                        <a-form-item class=\"mb\">\n                          <a-button type=\"danger\" @click=\"removeItem(item.id)\">移除</a-button>\n                        </a-form-item>\n                        <a-form-item class=\"mb\">\n                          <a-button type=\"primary\" @click=\"addCompany(item.id,index,'addLittle')\">确定</a-button>\n                        </a-form-item>\n                      </template>\n                      <a-icon\n                        type=\"ellipsis\"\n                        class=\"item-right-edit\"\n                        @click=\"updataDes(item.odNumber,item.odPromiseDueDate,index)\"\n                      />\n                    </a-popover>\n                    <a-button\n                      type=\"primary\"\n                      shape=\"circle\"\n                      icon=\"plus\"\n                      @click=\"addCompany(item.id,index,'addAll')\"\n                    ></a-button>\n                  </div>\n                </div>\n              </a-list-item>\n              <div v-if=\"loadingCompony && !busy\" class=\"demo-loading-container\">\n                <a-spin/>\n              </div>\n            </a-list>\n          </div>\n        </div>\n      </a-col>\n      <a-col :span=\"12\" class=\"dispatch-right\">\n        <div class=\"title\">\n          <span @click=\"showDrawer\">{{companyName}}</span>\n          <span @click=\"showDrawer\" v-if=\"visible\">\n            <a-icon type=\"double-right\"/>\n          </span>\n          <span @click=\"showDrawer\">\n            <a-icon type=\"double-left\"/>\n          </span>\n        </div>\n        <div class=\"order-list demo-infinite-container\">\n          <a-list :dataSource=\"companyList\">\n            <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n              <div class=\"list-item\">\n                <div class=\"item-left\">\n                  <div class=\"item-left-top\">\n                    <a href=\"javascript:;\">{{item.name}}__{{item.bom}}</a>\n                    <span>{{item.craft}}</span>\n                  </div>\n                  <ul class=\"item-left-desc\">\n                    <li>{{item.spec}}</li>\n                    <li>{{item.cz}}</li>\n                    <li>{{item.bmcl}}</li>\n             