 from 'vue-infinite-scroll'\nimport { postJson, get } from '@/api/axios.js';\nimport api from '@/api/index.js';\nimport moment from 'moment';\nconst formItemLayout = {\n  labelCol: { span: 4 },\n  wrapperCol: { span: 18 }\n}\nexport default {\n  directives: { infiniteScroll },\n  data() {\n    return {\n      factoryId: '',\n      dateFormat: 'YYYY-MM-DD',\n      formItemLayout,\n      visible: false,\n      busy: false,\n      loadingCompony: false,\n      dataCompany: [],\n      factoryOrderDetailPostBean: {\n        currentPage: 1,\n        pageSize: 15,\n        cz: '',\n        spec: '',\n        startTime: '',\n        endTime: '',\n      },\n      companyName: '请选择工厂',\n      companyList: [],\n      companyNameList: [],\n      updateList: {\n        index: -1,\n        num: 0,\n        date: ''\n      },\n      goodsInfo: {\n        currentPage: 1,\n        pageSize: 15\n      },\n      specification: [], // 产品规格\n      textures: [] // 产品材质\n    }\n  },\n  created() {\n    this.getFactoryOrder()\n    this.getCompanyList()\n    this.getListCompanySpringGoodsInfo()\n  },\n  methods: {\n    moment,\n    // 按条件搜索渠道商订单列表\n    inputChange(val, type) {\n      let factoryOrderDetailPostBean = this.factoryOrderDetailPostBean\n      factoryOrderDetailPostBean.currentPage = 1\n      switch (type) {\n        case \"spec\":\n          factoryOrderDetailPostBean.spec = this.specification[val]\n          break;\n        case \"cz\":\n          factoryOrderDetailPostBean.cz = this.textures[val]\n          break;\n        case \"date\":\n          factoryOrderDetailPostBean.startTime = val[0]\n          factoryOrderDetailPostBean.endTime = val[1]\n          break;\n      }\n      this.getFactoryOrder()\n    },\n    handleInfiniteOnLoad() {\n      // console.log(1);\n    },\n    // 移除订单\n    removeItem(id) {\n      var _this = this\n      this.$confirm({\n        title: '移除订单信息',\n        content: '您确定要删除这条订单消息吗?',\n        zIndex: 99999,\n        onOk() {\n          postJson(api.delFactoryOrder, { id: id }).then(res => {\n            if (res.data.code === 0) {\n              _this.updateList.index = -1\n              _this.$message.success('移除成功', 1)\n              _this.getCompanyList()\n            }\n          })\n        },\n        onCancel() { },\n      });\n    },\n    // 打开抽屉\n    showDrawer() {\n      this.visible = !this.visible\n    },\n    // 关闭抽屉\n    onClose() {\n      this.visible = false\n    },\n    // 分派订单到工厂\n    addCompany(id, index, type) {\n      let dataCompany = this.dataCompany\n      let companyList = this.companyList\n      let updateList = this.updateList\n      updateList.index = -1\n      let tar = companyList.filter(item => item.id === id)[0]\n      if (tar) {\n        if (type === 'addAll') {\n          tar.odNumber += dataCompany[index].odNumber\n          dataCompany.splice(index, 1)\n        } else {\n          if (updateList.num >= dataCompany[index].odNumber) {\n            dataCompany.splice(index, 1)\n          } else {\n            dataCompany[index].odNumber -= updateList.num\n          }\n          tar.odPromiseDueDate = updateList.date\n          tar.odNumber += updateList.num\n        }\n      } else {\n        if (type === 'addAll') {\n          companyList.push(dataCompany[index])\n          dataCompany.splice(index, 1)\n        } else {\n          const target = dataCompany[index]\n          if (updateList.num >= target.odNumber) {\n            target.odPromiseDueDate = updateList.date\n            companyList.push(target)\n            dataCompany.splice(index, 1)\n          } else {\n            let team = JSON.parse(JSON.stringify(target))\n            target.odNumber -= updateList.num\n            team.odNumber = updateList.num\n            team.odPromiseDueDate = updateList.date\n            companyList.push(team)\n          }\n        }\n      }\n    },\n    // 删除工厂中的订单\n    minusCompany(id, index) {\n      let dataCompany = this.dataCompany\n      let companyList = this.companyList\n      let target = dataCompany.filter(item => item.id === id)[0]\n      if (target) {\n        target.odNumber += companyList[index].odNumber\n      } else {\n        dataCompany.push(companyList[index])\n      }\n      companyList.splice(index, 1)\n    },\n    // 获取渠道商订单列表\n    getFactoryOrder() {\n      this.loadingCompony = true\n      postJson(api.getFactoryOrder, this.factoryOrderDetailPostBean).then(res => {\n        if (res.data.code === 0) {\n          this.dataCompany = res.data.data.records\n        }\n        this.loadingCompony = false\n      })\n    },\n    // 设置选中的订单产品的数量和交期\n    updataDes(num, data, index) {\n      let updateList = this.updateList\n      if (updateList.index === -1) {\n        updateList.index = index\n        updateList.num = num\n        updateList.date = data\n      } else {\n        updateList.index = -1\n      }\n\n    },\n    updateTime(dates, dateStrings) {\n      this.updateList.date = dateStrings\n    },\n    onChange(dates, dateStrings) { },\n    // 订单分派提交\n    submit() {\n      if (this.factoryId === '') {\n        this.$message.warning('请选择工厂')\n        return\n      }\n      let addFactorySaleOrderPostBean = {\n        factoryId: this.factoryId,\n        list: []\n      }\n      let list = addFactorySaleOrderPostBean.list\n      let companyList = this.companyList\n      companyList.forEach(item => {\n        list.push(\n          {\n            id: item.id,\n            number: item.odNumber,\n            odRequireDueDate: item.odPromiseDueDate\n          }\n        )\n      })\n      postJson(api.addFactoryOrder, addFactorySaleOrderPostBean).then(res => {\n        if (res.data.code === 0) {\n          this.$message.success('分派成功')\n          this.getFactoryOrder()\n          this.companyList = []\n        }\n      })\n    },\n    // 获取工厂\n    getCompanyList() {\n      get(api.companyList, { type: 'FACTORY' }).then(res => {\n        this.companyNameList = res.data.data\n      })\n    },\n    // 选取分配工厂\n    setFactoryId(id, name) {\n      this.factoryId = id\n      this.companyName = name\n      this.visible = false\n    },\n\n    // 列表商家所有弹簧商品的弹簧信息\n    getListCompanySpringGoodsInfo(currentPage = 1, pageSize = 15) {\n      get(api.getListCompanySpringGoodsInfo, { currentPage, pageSize }).then(res => {\n        if (res.data.code === 0) {\n          let data = res.data.data.records\n          let textures = []\n          let specification = []\n          for (const i in data) {\n            textures.push(data[i].cz)\n            specification.push(data[i].spec)\n          }\n          if (this.textures.length) {\n            this.textures.concat(textures)\n          } else {\n            this.textures = textures\n          }\n          if (this.specification.length) {\n            this.specification.concat(specification)\n          } else {\n            this.specification = specification\n          }\n          this.goodsInfo.currentPage += 1\n        }\n      })\n    },\n    // 选择框的下拉加载\n    currentPageChange(e) {\n      const boxHeigth = e.target.scrollHeight - 1\n      const box = {\n        scrollTop: e.target.scrollTop,\n      }\n      if (parseFloat(getComputedStyle(document.querySelector('.ant-select-dropdown-content')).height) + box.scrollTop > boxHeigth) {\n        this.getListCompanySpringGoodsInfo(this.goodsInfo.currentPage)\n      }\n    },\n  }\n}\n",{"version":3,"sources":["dispatch.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2MA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AA