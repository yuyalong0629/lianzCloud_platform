te>\n  <div class>\n    <a-form layout=\"horizontal\" style=\"margin-bottom:20px\">\n      <a-row>\n        <a-col :span=\"7\">\n          <a-form-item :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\" label=\"订单编号\">\n            <a-input\n              v-model=\"data.odOrderNumber\"\n              class=\"inputWidth\"\n              placeholder=\"请输入订单编号\"\n              @change=\"e=>inputChange(e.target.value,'odOrderNumber')\"\n            />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"7\">\n          <a-form-item :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\" label=\"交期\">\n            <a-range-picker\n              class=\"inputWidth\"\n              v-model=\"dateArr\"\n              @change=\"(data,dateing)=>inputChange(dateing,'date')\"\n              format=\"YYYY/MM/DD HH:mm:ss\"\n            />\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"7\">\n          <a-form-item :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\" label=\"产品规格\">\n            <a-select\n              style=\"width:200px\"\n              showSearch\n              :allowClear=\"true\"\n              v-model=\"data.spec\"\n              placeholder=\"请选择规格\"\n              @change=\"(val)=>{inputChange(val,'spec')}\"\n              @popupScroll=\"e=>currentPageChange(e)\"\n              :getPopupContainer=\"triggerNode => triggerNode.parentNode\"\n            >\n              <a-select-option v-for=\"item in odSpecList\" :key=\"item\">{{item}}</a-select-option>\n            </a-select>\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"3\">\n          <a-form-item>\n            <span style=\"cursor: pointer;\" @click=\"searchReset\">重置</span>\n          </a-form-item>\n        </a-col>\n      </a-row>\n    </a-form>\n  </div>\n</template>\n\n<script>\nimport { mixinGetProductAbout, mixinCurrentPage } from '@/views/sale/mixin/minxinSale'\nexport default {\n  props: {\n    typeInput: {\n      type: String,\n      default: '',\n      required: true\n    }\n  },\n  mixins: [mixinGetProductAbout, mixinCurrentPage],\n  data() {\n    return {\n      labelCol: {\n        span: 6\n      },\n      wrapperCol: {\n        span: 14\n      },\n      dateArr: [],\n      timer: '',\n      data: {\n        odOrderNumber: '',\n        spec: '',\n        startTime: '',\n        endTime: ''\n      }\n    }\n  },\n  created() {\n    this.getSpec()\n  },\n  methods: {\n    // 按条件搜索\n    inputChange(val, type) {\n      if (type === 'date') {\n        this.data.startTime = val[0]\n        this.data.endTime = val[1]\n      } else {\n        this.data[type] = val\n      }\n      // 输入框防抖\n      if (this.timer !== '') {\n        clearTimeout(this.timer)\n      }\n      this.timer = setTimeout(() => {\n        this.$emit('dataChange', this.data)\n      }, 500)\n    },\n    // 重置搜索条件\n    searchReset() {\n      this.data.odOrderNumber = ''\n      this.data.spec = ''\n      this.dateArr = []\n      this.data.endTime = ''\n      this.data.startTime = ''\n      this.$emit('dataChange', this.data)\n    },\n    currentPageChange(e) {\n      const result = this.getCurrentPageChange(e)\n      if (result) {\n        this.getSpec()\n      }\n    },\n    // spec的区别获取\n    getSpec() {\n      if (this.typeInput === 'distributors') {\n        this.getListCompanySpringSpecs(this.specCurrentPage)\n      } else if (this.typeInput === 'factory') {\n        this.getListCompanySpringSpecs(this.specCurrentPage)\n      } else if (this.typeInput === 'procurement') {\n        this.getFactoryMaterialRequireSpec(this.specCurrentPage)\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"~styles/variable.less\";\n\n.inputWidth {\n  .inputWidth();\n}\n</style>\n"]}]}