n\" type=\"delete\"/>\n          <span>删除</span>\n        </span>\n        <a-form layout=\"horizontal\" style=\"margin-bottom:20px\">\n          <a-row>\n            <a-col :span=\"8\">\n              <a-form-item\n                :label-col=\"formItemLayout.labelCol\"\n                :wrapper-col=\"formItemLayout.wrapperCol\"\n                label=\"订单编号\"\n              >\n                <a-input class=\"inputWidth\" placeholder=\"请输入订单编号\"/>\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"8\">\n              <a-form-item\n                :label-col=\"formItemLayout.labelCol\"\n                :wrapper-col=\"formItemLayout.wrapperCol\"\n                label=\"交期\"\n              >\n                <a-range-picker class=\"inputWidth\" format=\"YYYY/MM/DD\"/>\n              </a-form-item>\n            </a-col>\n            <a-col :span=\"8\">\n              <a-form-item\n                :label-col=\"formItemLayout.labelCol\"\n                :wrapper-col=\"formItemLayout.wrapperCol\"\n                label=\"产品规格\"\n              >\n                <a-input class=\"inputWidth\" placeholder=\"请选择产品规格\"/>\n              </a-form-item>\n            </a-col>\n          </a-row>\n        </a-form>\n        <a-table\n          :columns=\"columns\"\n          :dataSource=\"data\"\n          @change=\"onChangePage\"\n          :pagination=\"pagination\"\n          :rowSelection=\"rowSelection\"\n          :loading=\"loading\"\n        ></a-table>\n      </a-col>\n    </a-row>\n  </div>\n</template>\n\n<script>\nconst formItemLayout = {\n  labelCol: { span: 6 },\n  wrapperCol: { span: 18 }\n}\nimport { columns } from './columns';\nimport infiniteScroll from 'vue-infinite-scroll'\nexport default {\n  directives: { infiniteScroll },\n  data() {\n    return {\n      formItemLayout,\n      columns,\n      data: [],\n      busy: false,\n      loadingCompony: false,\n      dataCompany: ['工厂1', '工厂2'],\n      loading: false,\n      selectedRowKeys: [],\n      pagination: {\n        total: 0,\n        defaultPageSize: 15,\n        defaultCurrent: 1,\n        showQuickJumper: true,\n        hideOnSinglePage: true\n      },\n    }\n  },\n  computed: {\n    rowSelection() {\n      const { selectedRowKeys } = this;\n      return {\n        selectedRowKeys,\n        onChange: this.onChangeTab,\n        hideDefaultSelections: false\n      };\n    }\n  },\n  methods: {\n    onSearch(value) {\n      console.log(value);\n    },\n    onChangeTab(selectedRowKeys, selectedRows) {\n      this.selectedRowKeys = selectedRowKeys;\n      this.selectedRows = selectedRows\n    },\n    // 翻页\n    onChangePage(pagination, filters, sorter) {\n      console.log(pagination);\n    },\n    handleInfiniteOnLoad() {\n      const data = this.dataCompany\n      // this.loadingCompony = true\n      if (data.length > 14) {\n        this.$message.warning('Infinite List loaded all')\n        this.busy = true\n        this.loadingCompony = false\n        return\n      }\n    },\n  }\n}\n</script>\n\n<style lang='less'>\n@import \"~styles/variable.less\";\n\n.inputWidth {\n  .inputWidth();\n}\n\n.purchase-left {\n  border-top: 1px solid #eee;\n  border-right: 1px solid #eee;\n  border-bottom: 1px solid #eee;\n  min-height: 840px;\n  max-height: 840px;\n  overflow-y: auto;\n  padding: 10px;\n  box-sizing: border-box;\n}\n.purchase-right {\n  padding: 10px;\n}\n.delete-btn {\n  margin: 0 20px;\n  cursor: pointer;\n  .delete-icon {\n    color: #f00;\n    margin-right: 3px;\n  }\n}\n.demo-infinite-container {\n  border-radius: 4px;\n  overflow: auto;\n  padding: 8px 24px;\n  height: 100%;\n  max-height: 840px;\n}\n.demo-loading-container {\n  position: absolute;\n  bottom: 40px;\n  width: 100%;\n  text-align: center;\n}\n.ant-list-split .ant-list-item{\n  text-align: center;\n}\n</style>\n"]}]}